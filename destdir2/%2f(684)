<script>
function populateAmbiguousSearchParameters(searchBoxNo) {
	var key='keyword'+searchBoxNo;
	var value='keywordValue'+searchBoxNo;
	var qs= document.getElementById(key).value;
	qs = qs.replace(/^\s+/, '');	
	qs = qs.replace(/\s+$/, '');
	document.getElementById(value).value=qs;
	if (qs == '') return false;
}
</script>

