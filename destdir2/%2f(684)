<script language="javascript" type="text/javascript">
	var url = "/App/ViewDHTMLCalendar?z=a8b4&r=p";
	var sslUrl = "https://www.orbitz.com/Secure/ViewSecureCalendar?z=a8b5&r=q";
	var calContainer = document.getElementById('calDiv');
	if (calContainer&&document.createElement){
		var calIframe=document.createElement('IFRAME');
		calIframe.id='calIframe';
		calIframe.name='jsCal';
		calIframe.height=5;
		calIframe.width=5;
		calIframe.scrolling='no';
		calIframe.frameBorder=0;
		if (window.location.protocol == "https:"){
			calIframe.src = sslUrl;
		}else{
			calIframe.src = url;
		}
		calContainer.appendChild(calIframe);
	}
</script>
			
		
		
