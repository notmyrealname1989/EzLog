<script type="text/javascript">


function backButtonScroll(scrollPos, inFrame){
	top.backButtonScroll(scrollPos, inFrame);
}

function resizeFrame(){
	var newHeight = getElementPosition(document.getElementById('resultsEnd')).top + 25;
	if (newHeight < 725) {
		
		newHeight = 725;
	}
	
	if(parent.document.getElementById('resultsFrame')!=null)
	{
		parent.document.getElementById('resultsFrame').height = newHeight;
	}
	if(parent.document.getElementById('hotelResultsFrame')!=null)
	{
		parent.document.getElementById("hotelResultsFrame").height = parent.document.getElementById("hotelResultsFrame").contentWindow.document.body.scrollHeight + "px";
	}

}

/*function printVersion(){
	if(parent.document.getElementById('resultsFrame')!=null) {
		var printVersion=document.getElementById("resultSet").innerHTML;
		top.document.getElementById("printVersion").innerHTML = printVersion;
	}
}*/



function selectHotelFromMatrix(clickID) {
	var vs = document.getElementById("valueSorted");
	if(vs) {
		vs.innerHTML = '';
		var rs = document.getElementById('resultId' + clickID)
		if(rs) {
			var rsi = rs.innerHTML;
			hideElements("result" + clickID);
			vs.innerHTML = rsi;
			showElements("valueSorted");
			top.hideElements("unavailableHotel");
		}
	}

}

</script>

	
		

