<script language="JavaScript" type="text/javascript">
function doGRPopLink(sLink) {
	var bReturn;
	var bIsPopup;
	if (bIsPopup == undefined) {
		bIsPopup = false;
	}
	
	sLink += "&isPopup=" + (bIsPopup ? "true" : "false");
	if (!bIsPopup) {
		var sWin = 'guestReviewsOrbitz'.substr(0,16);
		popUpGen(sLink,'480','600',sWin,false,"oGRPop");
	} else {
		document.location.href = sLink;
	}
	return false;
}
        
function doGRPopReview(s) {
	return doGRPopLink('/App/ViewGuestReviews?z=95d9&r=4m&reviewSelectedIndex='+s);
}
function doGRPopReview(s,v) {
	return doGRPopLink('/App/ViewGuestReviews?z=95da&r=4n&reviewSelectedIndex='+s+'&hotelEntryKey='+v);
}
function doGRPopReviewFromResults(s) {
	return doGRPopLink('/App/PrepareGuestReviews?z=95db&r=4o&hotelEntryKey='+s);
}
function doGRPopReviewFromDealsLandingPage(s) {
	return doGRPopLink('/App/PrepareDealsLandingGuestReviews?z=95dc&r=4p&hotelEntryKey='+s);
}
function doGRPopReviewAll() {
	return doGRPopLink('/App/ViewGuestReviews?z=95dd&r=4q');
}
function doGRPopReviewAll(s) {
	return doGRPopLink('/App/ViewGuestReviews?z=95de&r=4r&hotelEntryKey='+s);
}
function doGRPopReviewSegment(s) {
    return doGRPopLink('/App/PerformHotelGuestReviewFilter?z=95df&r=4s&segmentType='+s);
}
function doGRPopReviewSegment(s,v) {
    return doGRPopLink('/App/PerformHotelGuestReviewFilter?z=95e0&r=4t&segmentType='+s+'&hotelEntryKey='+v);
}
function doGRPopReviewSort(s) {
	return doGRPopLink('/App/PerformHotelGuestReviewDetailSort?z=95e1&r=4u&sorttype='+s);
}
function doGRPopExpert() {
	return doGRPopLink('/App/ViewExpertReview?z=95e2&r=4v');
}
function doGRPopExpert(s) {
	return doGRPopLink('/App/ViewExpertReview?z=95e3&r=4w&hotelEntryKey='+s);
}
function doGRPopExpertFromResults(s) {
	return doGRPopLink('/App/PrepareExpertReview?z=95e4&r=4x&hotelEntryKey='+s);
}
function doGRPopHint() {
	return doGRPopLink('/App/ViewHints?z=95e5&r=4y');
}
function doGRPopHint(s) {
	return doGRPopLink('/App/ViewHints?z=95e6&r=4z&hotelEntryKey='+s);
}
function doGRPopHintSort(s) {
	return doGRPopLink('/App/PerformHotelGuestHintsSort?z=95e7&r=50&sorttype='+s);
}
function doGRPopHintCategory(s) {
	return doGRPopLink('/App/PerformHotelGuestHintFilter?z=95e8&r=51&categoryType='+s);
}
function testUserScoreAPH(s,v,p){  
		return doGRPopLink('/App/PrepareAPHGuestReview?z=95e9&r=52&link=user_score&origin=pkg.aph&pkgPageNum='+p+'&pkgRowNum='+v+'&pkgIndex=1&hotelEntryKey='+s);
}
function userScore(s,v,p){
	return doGRPopLink('/App/PrepareHPCGuestReview?z=95ea&r=53&link=user_score&origin=pkg.hpc&pkgPageNum='+p+'&pkgRowNum='+v+'&pkgIndex=1&hotelEntryKey='+s);
}

