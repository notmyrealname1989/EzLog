<script type="text/javascript">//<![CDATA[
var dynRHFRec;
var rhfRecLoaded = false;
n2RunThisWhen(
    "lastlibraryloaded",
    function() {
        n2RunIfLoaded(
            "dynUpdate",
            function() {

dynRHFRec = new N2DynUpObject();
dynRHFRec.setHandler('/gp/history/external/rhf-rec-handler-t1','sd');
dynRHFRec.setLoadingMessage('<br /><center><img src="http://g-ecx.images-amazon.com/images/G/01/x-locale/communities/tags/narrowtimer_transparent._V47062518_.gif" width="110" height="10" border="0" /></center>');
dynRHFRec.setTargetElementID("rhf_rec_div");
dynRHFRec.setReftag("pd_rhf_dyn");
dynRHFRec.onRequestSuccess = function (aHTML, aJSFunctions, status) {
    if (aHTML.length == 0) {
        goN2U.getElement('rhf_rec_cell').style.display = 'none';
        goN2U.getElement('rhfUpsellColumnContent').style.display = 'none';
    }
    else if (aHTML.length == 1 && aHTML[0] == '') {
        goN2U.getElement('rhf_rec_cell').style.display = 'none';
        goN2U.getElement('rhfUpsellColumnContent').style.display = 'none';
    }
    else {
        goN2U.getElement('rhf_rec_cell').style.display = '';
        goN2U.getElement('rhfUpsellDescription').style.display = '';
    }
    goN2U.getElement('rhf_rec_div').innerHTML = aHTML[0];
    if (aHTML[1]) {
        goN2U.getElement('rhfUpsellColumnTitle').innerHTML = aHTML[1];
    }
    else {
        goN2U.getElement('rhfUpsellColumnTitle').innerHTML = '';
    }
    rhfRecLoaded = true;
}

n2RunThisWhen('onload', 
              function() { 
                goN2U.getElement('rhfUpsellColumnContent').style.display = '';
              } );

dynRHFRec.onRequestFailure = function(sMessage, status) {
    goN2U.getElement('rhf_rec_cell').style.display = 'none';
    goN2U.getElement('rhfUpsellColumnContent').style.display = 'none';
}

function rhf_js_scrollAction() {
    var yPosition = goN2U.getScrollTop() + goN2U.getInsideWindowHeight();
    var rhfElement = goN2U.getElement("rhf_table");
    var rhfPosition = goN2U.getPageElementTop(rhfElement);
    if (!rhfRecLoaded) {
        goN2U.getElement('rhf_rec_div').innerHTML = '<br /><div style="text-align:center;"><img src="http://g-ecx.images-amazon.com/images/G/01/x-locale/communities/tags/narrowtimer_transparent._V47062518_.gif" width="110" height="10" border="0" /></div>';
        goN2U.getElement('rhfUpsellColumnTitle').innerHTML = 'Loading Recommendations...';
        if (rhfPosition - yPosition < 400) {
            dynRHFRec.requestUpdate("rhf","0345494997","0345494997");
        }
    }
}

function rhf_js_pageLoad() {
    goN2U.getElement('rhf_rec_cell').style.display = '';
    goN2U.getElement('rhfUpsellColumnContent').style.display = '';
    rhf_js_scrollAction();
}

goN2U.addHandler(window,"scroll",rhf_js_scrollAction);
goN2U.addHandler(window,"load",rhf_js_pageLoad);

            }, 'RHF Recs - dynamic');
    }, 'RHF Recs - last library loaded');

//]]></script>
