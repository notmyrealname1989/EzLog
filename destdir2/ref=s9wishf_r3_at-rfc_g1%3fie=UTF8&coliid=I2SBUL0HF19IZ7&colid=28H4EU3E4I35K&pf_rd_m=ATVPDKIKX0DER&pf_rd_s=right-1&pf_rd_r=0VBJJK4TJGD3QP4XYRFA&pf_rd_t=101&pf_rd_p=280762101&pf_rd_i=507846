<script>
Shoveler.prototype.logInteraction = function (type, id) {
// client-side logging
if ((this.enableLogging) && (this.enableLogging == 1)) {
    this.preloadShvlLoader(this.asinList,0,1);
    if (window.clientLogger){
        var params = {};
        params[ "ASIN" ] = this.mainAsin;
        params[ type ] = id;
        params[ "REFTAG" ] = this.refTag;
        clientLogger.sendCLOGEntry("similarities", "pd_sim_cloud", params);
    }
    if (this.shvlLoader) {
        this.shvlLoader.requestUpdate("shvlLog",type+":"+this.refTag+":"+id,"a",{});
    }
}
}
</script>














