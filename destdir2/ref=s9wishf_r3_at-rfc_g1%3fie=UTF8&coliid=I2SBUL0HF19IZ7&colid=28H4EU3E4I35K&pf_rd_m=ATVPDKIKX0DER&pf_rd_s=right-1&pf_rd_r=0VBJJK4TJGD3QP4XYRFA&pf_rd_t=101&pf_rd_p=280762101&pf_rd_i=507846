<script type="text/javascript">
var OtherEditions = {
  isExpanded: false,
  toggle: function() {
    var elements = this.getElements();
    var style = '';
    var moreRow = document.getElementById('oeShowMore')
    var fewerRow = document.getElementById('oeShowFewer')
    if (this.isExpanded) {
      style = 'none';
      moreRow.style.display = '';
      fewerRow.style.display = 'none';
    } else {
      moreRow.style.display = 'none';
      fewerRow.style.display = '';
    }
    for (var i = 0; i < elements.length; i++) {
      elements[i].style.display = style;
    }
    this.isExpanded = !this.isExpanded;
    return false;
  },
  getElements: function() {
    var table = document.getElementById('oeTable') || document;
    var elements = table.getElementsByTagName('tr');
    // based on Dustin Diaz's getElementsByClass
    var className = new RegExp('(^|\\s)oeMore(\\s|$)');
    var matches = [];
    for (var i = 0; i < elements.length; i++) {
      if (className.test(elements[i].className)) {
        matches.push(elements[i]);
      }
    }
    return matches;
  }
};
</script>

