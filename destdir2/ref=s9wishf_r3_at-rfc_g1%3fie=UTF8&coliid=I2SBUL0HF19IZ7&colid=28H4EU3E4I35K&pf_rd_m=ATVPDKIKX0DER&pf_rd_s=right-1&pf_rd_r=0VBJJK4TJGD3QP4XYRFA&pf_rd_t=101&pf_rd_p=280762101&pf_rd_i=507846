        var synchronizedDropdown = document.handleBuy.elements["dropdown-selection"];
        if (synchronizedDropdown instanceof Array) {
            for (d in synchronizedDropdown) {
                d.selectedIndex = dropdownElement.selectedIndex;
            }
        } else {
            synchronizedDropdown.selectedIndex = dropdownElement.selectedIndex;
        }
        document.handlePrimeBuy.submit();
    }
}

