<script type="text/javascript">
var cdLocalizedStrings = new Object();

function cdSetLocalizedString(key, value) {
  cdLocalizedStrings[key] = value;
}

function cdGetLocalizedString(key, replacements) {
  var str = cdLocalizedStrings[key];

  if ( !str ) {
      return key;
  }

  if ( replacements ) {
    for ( var replace in replacements ) {
      var replacementKey = "${"+replace+"}";
      str = str.split(replacementKey).join(replacements[replace]);
    }
  }

  return str;
}
</script>


<script><!--

cdSetLocalizedString('err_thread_post_topic_empty', 'The discussion topic is empty. Please add text to it.');
cdSetLocalizedString('err_thread_post_topic_n_too_long', 'The discussion topic is ${length} characters longer than the maximum topic length. Please edit it.');
cdSetLocalizedString('err_thread_post_body_n_too_long', 'The post is ${length} characters longer than the maximum post length. Please edit it.');
cdSetLocalizedString('alt_cancel', 'Cancel');
cdSetLocalizedString('alt_start_the_discussion', 'Start the discussion');
cdSetLocalizedString('txt_guidelines', 'Guidelines');
cdSetLocalizedString('txt_thread_post_start_a_new_discussion', 'Start a new discussion');
cdSetLocalizedString('txt_thread_post_topic', 'Topic');
cdSetLocalizedString('txt_thread_post_first_post', 'First post');
cdSetLocalizedString('txt_thread_post_track_checkbox', 'Receive e-mail when new posts are made');
cdSetLocalizedString('txt_thread_post_prompt_signin', 'Prompts for sign-in');
cdSetLocalizedString('txt_thread_post_prompt_realname', 'Prompts for Real Name');
cdSetLocalizedString('err_thread_post_no_purchase', 'You must have purchased items from Amazon to post.');
cdSetLocalizedString('err_thread_post_banned', 'You must be in good standing in the Amazon community to post.');
cdSetLocalizedString('err_thread_post_pmf', 'Your message will not be posted. Please see our ${guidelinesHref}Guidelines</a> regarding objectionable content.');
cdSetLocalizedString('err_thread_post_subscribe_on_post', 'Sorry, an error occurred while adding you to the tracking list. (Your message has been posted successfully.) Please try again later.');
cdSetLocalizedString('err_thread_post_topic_too_long', 'The discussion topic is longer than the maximum topic length of 255 characters. Please edit it.');
cdSetLocalizedString('err_thread_post_body_too_long', 'The discussion body is longer than the maximum body length of 16 kilobytes. Please edit it.');
cdSetLocalizedString('err_thread_post_body_empty', 'The discussion body is empty. Please add text to it.');
cdSetLocalizedString('err_thread_post_general', 'Sorry, an error occurred when we tried to process your request. Rest assured, we\'re working to resolve the problem as soon as possible.');

  var cdDisablePostForm = 0;
  function cdOpenPostBox() {
    document.getElementById('cdPostBox').style.display = 'block';
    document.getElementById('startDiscussionBtn').style.display = 'none';
    return false;
  }
 
  function cdOpenPostBoxStatic() {
    document.getElementById('cdPostBox').style.display = 'block';
    document.getElementById('startDiscussionBtn').style.display = 'none';
    return false;
  }
 
  function cdFocusPostTitle() {
    document.getElementById('cdPostBoxTitleInput').focus();
    return false;
  }

  function cdClosePostBox() {
    document.getElementById('cdPostBox').style.display = 'none';
    document.getElementById('startDiscussionBtn').style.display = 'inline';
    document.getElementById('cdPostBoxForm').reset();
    cdDisablePostForm = 0;
    return false;
  }
                                                                                                                  
  function cdPostCancelKey(e) {
    var code;
    if (e.keyCode) code = e.keyCode;
    else if (e.which) code = e.which;
    if (code == 32) {
      cdClosePostBox();
      return false;
    }
  }

  function cdCheckPost(form) {
    var sub = '' + form.subjectText.value;
    if ( (sub == '') || (sub.match(/^\s*$/)) ) {
      alert("The discussion topic is empty. Please add text to it.");
      return false;
    }
    if ( sub.length > 255 ) {
      alert( cdGetLocalizedString('err_thread_post_topic_n_too_long', {'length':(sub.length - 255)}) );
      return false;
    }
                                                                                                                  
    var body = '' + form.bodyText.value;
    if ( (body == '') || (body.match(/^\s*$/)) ) {
      alert("The discussion body is empty. Please add text to it.");
      return false;
    }
    if ( body.length > 16000 ) {
      alert( cdGetLocalizedString('err_thread_post_body_n_too_long', {'length':(body.length - 16000)}) );
      return false;
    }
    if ( cdDisablePostForm++ ) {
      return false;
    }
                                                                                                                  
    return true;
  }
--></script>

