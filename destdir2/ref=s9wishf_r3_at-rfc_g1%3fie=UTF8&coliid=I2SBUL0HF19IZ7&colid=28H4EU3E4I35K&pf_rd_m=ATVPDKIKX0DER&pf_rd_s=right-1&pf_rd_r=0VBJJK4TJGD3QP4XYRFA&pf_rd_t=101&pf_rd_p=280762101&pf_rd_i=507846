  aHandlers: [],
  runThisWhen: function (sWhen, fFn, sComment) {
    if ((typeof fFn != 'function') || fFn == null) return false;
    this.aHandlers[this.aHandlers.length] = { sWhen: sWhen.toLowerCase(), fFn: fFn, sComment: sComment };
    return true;
  },
  isReady: function() {return false;}
};
