<td valign="top">








<script language="Javascript" type="text/javascript">


function ownItImageID(ownIt) {
    return "ratings-detail-check-" + ownIt;
}
function starsImageID(rating) {
    return "ratings-detail-stars-" + rating;
}

function getOwnItImage(ownIt) {
    return goN2U.preloadImages[ownItImageID(ownIt)];
}

function getStarsImage(rating) {
    return goN2U.preloadImages[starsImageID(rating)];
}

var checkboxImages = new Array(
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/check-off._V47082251_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/check-on._V47082089_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/check-hover._V47082329_.gif"
                );

var starImages = new Array(
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/unrated._V47082245_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/1-star._V47082241_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/2-star._V47082180_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/3-star._V47060083_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/4-star._V47082181_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/5-star._V47060077_.gif",
                "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/nav2/clear._V47060355_.gif"
                );

var ratingsDynamicUpdate;

function preloadNav2RatingsBar() {
    for (i=0;i < 3;i++) {
        goN2U.preloadImage(checkboxImages[i],ownItImageID(i));
    }
    for (i=0;i < 6;i++) {
        goN2U.preloadImage(starImages[i],starsImageID(i));
    }

    ratingsDynamicUpdate = new N2DynUpObject();

    ratingsDynamicUpdate.onRequestSuccess = function (aHTML,aJSFunctions,nStatus,sRequestID) {
        this._updateContent("messages." + this.sTargetID,starMessages[7]);
    }
    
    ratingsDynamicUpdate.setHandler('/gp/yourstore/ratings/nav2-submit','sd');
    ratingsDynamicUpdate.cacheResponses(false);
    ratingsDynamicUpdate.setLoadingMessage("");
}

n2RunThisWhen("lastlibraryloaded",function() {preloadNav2RatingsBar();},"Preload ratings bar images");

var starMessages = new Array(
            "Unrated",
            "I hate it",
            "I don't like it",
            "It's OK",
            "I like it",
            "I love it",
            "<strong>Rate this item</strong>",
            "&nbsp;Saved",
            "Your Rating"
            );
var ownItMessages = new Array(
            "I own it",
            "Saved");

var starMap = new Array(
        '0,0,12,12',
        '12,0,24,12',
        '24,0,36,12',
        '36,0,48,12',
        '48,0,60,12');

var savedRatings = new Object();
var oldRatings = new Object();
var changedRatings = new Object();
var savedNotInterested = new Object();
var savedIsOwned = new Object();
var oldIsOwned = new Object();
var savedExcluded = new Object();
var starTwinkler = new Object();
var clearTwinkler = new Object();
var msgTwinkler = new Object();

var starDelayTime = 500;


function amz_js_clickIsOwned(asin) {
  isOwned = (savedIsOwned[asin] ? 0 : 1);
  savedIsOwned[asin] = isOwned;

  amz_js_swapIsOwned(asin, isOwned);
}

function amz_js_isOwnedMouseOver(asin) {
    if (!savedIsOwned[asin]) {
        amz_js_swapIsOwned(asin, 2);
    }
    document.getElementById("messages.saved." + asin).innerHTML = "";
}

function amz_js_isOwnedMouseOut(asin) {
    if (!savedIsOwned[asin]) {
        amz_js_swapIsOwned(asin, 0);
    }
}


function amz_js_sendRating(asin, ratingType, ratingValue) {
  
  var ratingParam = asin
                    + ":"
                    + ratingType
                    + ":"
                    + ratingValue
                    + ":"
                    + ""
                    + ":"
                    + "002-7818855-9196047";
  ratingsDynamicUpdate.requestUpdate("submitRating",asin,"a",ratingParam);
}


function amz_js_swapStars(asin, rating) {
  if (rating == undefined) {
    rating = savedRatings[asin];
  }
  if (rating == undefined) rating = 0;
  document.images["stars." + asin].src = getStarsImage(rating).src;
}

function amz_js_swapStarMsgs(asin, rating) {
  if (rating == undefined) {
    if ( undefined != savedRatings[asin] && 0 < savedRatings[asin] ) {
      document.getElementById("messages." + asin).innerHTML = starMessages[8];
    }
    else {
      document.getElementById("messages." + asin).innerHTML = starMessages[6];
    }
  }
  else {
    document.getElementById("messages." + asin).innerHTML = starMessages[rating];
  }
}

function amz_js_swapIsOwnedMsgs(asin, isOwned) {
    document.getElementById("ownIt.messages." + asin).innerHTML = ownItMessages[isOwned];
}

function amz_js_swapIsOwned(asin, isOwned) {
    document.images["isOwned." + asin].src = getOwnItImage(isOwned).src;
}

function amz_js_sendStars(asin, rating) {
  oldRatings[asin] = savedRatings[asin];
  savedRatings[asin] = rating;
  changedRatings[asin] = 1;
  if (rating > 0) {
    if (clearTwinkler[asin] != 0) {
      window.clearTimeout(clearTwinkler[asin]);
      clearTwinkler[asin] = 0;
    }
    document.getElementById("messages." + asin).innerHTML = starMessages[8];
    amz_js_showClear(asin);
  }
  else {
    document.getElementById("messages." + asin).innerHTML = starMessages[6];
    amz_js_hideClear(asin);
  }
  ratingsDynamicUpdate.setTargetElementID("saved." + asin);

  ratingsDynamicUpdate.onRequestFailure = function (sMessage,nStatus,sRequestID) {
        savedRatings[asin] = oldRatings[asin];
        amz_js_swapStars(asin, savedRatings[asin]);
  }

  amz_js_sendRating(asin, 'onetofive', rating);
}

function amz_js_sendIsOwned(asin) {
  oldIsOwned[asin] = savedIsOwned[asin];
  savedIsOwned[asin] = (savedIsOwned[asin] ? 0 : 1);
  
  rating = (savedIsOwned[asin] ? 'OWN' : 'NONE');
  amz_js_swapIsOwned(asin,savedIsOwned[asin]);
  
  ratingsDynamicUpdate.setTargetElementID("saved." + asin);

  ratingsDynamicUpdate.onRequestFailure = function (sMessage,nStatus,sRequestID) {
        savedIsOwned[asin] = oldIsOwned[asin];
        amz_js_swapIsOwned(asin,savedIsOwned[asin]);
  }

  amz_js_sendRating(asin, 'owned', rating);
}

function amz_js_starMouseOver(asin, rating) {
  if (starTwinkler[asin] != 0) {
    window.clearTimeout(starTwinkler[asin]);
    starTwinkler[asin] = 0;
  }
  if (msgTwinkler[asin] != 0) {
    window.clearTimeout(msgTwinkler[asin]);
    msgTwinkler[asin] = 0;
  }
  if (clearTwinkler[asin] != 0) {
    window.clearTimeout(clearTwinkler[asin]);
    clearTwinkler[asin] = 0;
  }
  amz_js_swapStars(asin, rating);
  amz_js_swapStarMsgs(asin, rating);
  document.getElementById("messages.saved." + asin).innerHTML = "";
  if (savedRatings[asin]) {
    amz_js_showClear(asin);
  }
}

function amz_js_starMouseOut(asin,e) {
  starTwinkler[asin] = window.setTimeout("amz_js_swapStars('"+asin+"')",starDelayTime);
  msgTwinkler[asin] = window.setTimeout("amz_js_swapStarMsgs('"+asin+"')",starDelayTime);
  clearTwinkler[asin] = window.setTimeout("amz_js_hideClear('"+asin+"')",starDelayTime);
}

function amz_js_hideClear(asin) {
    var clearID = "clearStars." + asin;
    document.getElementById(clearID).style.display = "none";
}

function amz_js_showClear(asin) {
    var clearID = "clearStars." + asin;
    var starID = "stars." + asin;
    var top = goN2U.getPageElementTop(document.getElementById(starID));
    var left = goN2U.getPageElementLeft(document.getElementById(starID));
    var clear = document.getElementById(clearID);
    clear.style.top = (top - 1);
    clear.style.left = (left - 16);
    clear.style.display = "inline";
}



function amz_js_showIsOwned(asin, isOwned) {
  var imageID = "isOwned." + asin;
  
  document.write('<a href="#" onclick="amz_js_sendIsOwned(' + "'" + asin + "'" + '); return false;" ' +
                 ' onfocus="amz_js_isOwnedMouseOver(' + "'" + asin + "'" + ');" ' +
                 ' onblur="amz_js_isOwnedMouseOut(' + "'" + asin + "'" + ');" >');
  document.write('<img tabindex="0" src="' + checkboxImages[isOwned] + '" ' +
                 ' alt="' + ownItMessages[0] + '" ' +
                 ' id="' + imageID +'" ' +
                 ' onmouseover="amz_js_isOwnedMouseOver(' + "'" + asin + "'" + ');" ' +
                 ' onmouseout="amz_js_isOwnedMouseOut(' + "'" + asin + "'" + ');" ' +
                 ' border="0" valign="bottom" align="absmiddle" height="14" width="14" />');
  document.write('<'+'/a>');
}


function amz_js_showStars(asin, rating) {
    var starID = "stars." + asin;
    var clearID = "clearStars." + asin;
    var ratingsBarID = "ratingsBar." + asin;
    var starMapID = "starMap." + asin;

    starTwinkler[asin] = 0;
    msgTwinkler[asin] = 0;
    clearTwinkler[asin] = 0;

    document.write('<map name="starmap' + asin +'" id="'+ starMapID + '">');
    var i = 0;
    for (i = 0; i < 5; i++) {
        document.write('<area shape="rect" ' +
                       ' coords="' + starMap[i] + '" ' +
                       ' onmouseover="amz_js_starMouseOver(' + "'" + asin + "'," + (i+1) + ');" ' +
                       ' onfocus="amz_js_starMouseOver(' + "'" + asin + "'," + (i+1) + ');" ' +
                       ' onmouseout="amz_js_starMouseOut(' + "'" + asin + "'" + ');" ' +
                       ' onblur="amz_js_starMouseOut(' + "'" + asin + "'" + ');" ' +
                       ' onclick="amz_js_sendStars(' + "'" + asin + "'," + (i+1) + '); return false;" ' +
                       ' alt="' + starMessages[i+1] + '" ' +
                       ' href="#" />' );
    }
    document.write('<'+'/map>');

    document.write('<span id="' + ratingsBarID + '">');

    var top = goN2U.getPageElementTop(document.getElementById(starID));
    var left = goN2U.getPageElementLeft(document.getElementById(starID));
    document.write('<span ' +
                   ' style="display:none; position: absolute; left:"' + (left-16) + '; top:' + top + ';" ' +
                   ' id="' + clearID + '" >');

    document.write('<a href="#" ' +
                   ' onmouseout="amz_js_starMouseOut(' + "'" + asin + "'" + ');" ' +
                   ' onblur="amz_js_starMouseOut(' + "'" + asin + "'" + ');" ' +
                   ' onmouseover="amz_js_starMouseOver(' + "'" + asin + "'" + ',0);" ' +
                   ' onfocus="amz_js_starMouseOver(' + "'" + asin + "'" + ',0);" ' +
                   ' onclick="amz_js_sendStars(' + "'" + asin + "'" + ',0); return false;" >');

    document.write('<img src="' + starImages[6] + '" ' +
                   ' alt="' + starMessages[0] + '" ' +
                   ' valign="bottom" align="absmiddle" border="0" >');
    document.write('<'+'/a>');
    
    document.write('<'+'/span>');

    document.write('<img src="' + starImages[rating] + '" ' +
                   ' valign="bottom" align="absmiddle" border="0" usemap="#starmap' + asin + '" ' +
                   ' id="' + starID + '" />');

    document.write('<'+'/span>');
}


function amz_js_showMessages(asin, rating) {
    document.write("<span id=\"saved." + asin + " style=\"display:none\"></span>");
    document.write("<span id=\"messages." + asin + "\">" + ((rating > 0)?starMessages[8]:starMessages[6])  + "</span>");
    document.write("<span id=\"messages.saved." + asin + "\" style=\"color: #990000\"></span>");
}

function amz_js_showIsOwnedMessages(asin) {
    document.write("<span style=\"cursor: pointer;cursor: hand;\" id=\"ownIt.messages." + asin + "\" ");
    document.write("onclick=\"amz_js_sendIsOwned('" + asin + "', 'alt');\" ");
    document.write("onmouseover=\"amz_js_isOwnedMouseOver('" + asin + "', 'alt');\" ");
    document.write("onmouseout=\"amz_js_isOwnedMouseOut('" + asin + "', 'alt');\" ");
    document.write(" >" + ownItMessages[0] + "</span>");
}


</script>

<script language="Javascript" type="text/javascript">
var asin = '0345494997';
var rating = 0;
var isOwned = 0;
var notInterested = 0;
var excluded = 0;

savedRatings[asin] = rating;
savedNotInterested[asin] = notInterested;
savedIsOwned[asin] = isOwned;
savedExcluded[asin] = excluded;
</script>
		<div class="rateThisItem"><script language="Javascript" type="text/javascript">amz_js_showMessages(asin,rating);</script></div>
    
</td>
</tr><tr><td valign="top">






<script language="Javascript" type="text/javascript">
var asin = '0345494997';
var rating = 0;
var isOwned = 0;
var notInterested = 0;
var excluded = 0;

savedRatings[asin] = rating;
savedNotInterested[asin] = notInterested;
savedIsOwned[asin] = isOwned;
savedExcluded[asin] = excluded;
</script>
<div class="rateThisItem">
<script language="Javascript" type="text/javascript">
n2RunThisWhen("lastlibraryloaded",function() {
    amz_js_showStars(asin, rating);
    document.write("&nbsp;&nbsp;");
    amz_js_showIsOwned(asin, isOwned);
    document.write("&nbsp;");
    amz_js_showIsOwnedMessages(asin);
},"display detail page ratings bar");
</script>
</div>
