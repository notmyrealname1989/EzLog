<script>
Shoveler.prototype.logInteraction = function (type, id) {
// client-side logging
if ((this.enableLogging) && (this.enableLogging == 1)) {
    this.preloadShvlLoader(this.asinList,0,1);
    if (window.clientLogger){
        var params = {};
        params[ "ASIN" ] = this.mainAsin;
        params[ type ] = id;
        params[ "REFTAG" ] = this.refTag;
        clientLogger.sendCLOGEntry("similarities", "pd_sim_cloud", params);
    }
    if (this.shvlLoader) {
        this.shvlLoader.requestUpdate("shvlLog",type+":"+this.refTag+":"+id,"a",{});
    }
}
}
</script>













<script>
function shovelerMain (detectBuyBox,firstLoad) {
    for (var i = 0; i < Shoveler.Instances.length; i++) {
        if (detectBuyBox) { Shoveler.Instances[i].buyBoxDetect(); }
        Shoveler.Instances[i].shovelResize(firstLoad);
    }
}

n2RunThisWhen("lastlibraryloaded",function() {shovelerMain(0,true);},"data load");
n2RunThisWhen("onload",function() {shovelerMain(1,true);},"data load");
if (goN2U.isSafari()) { N2ChainEventHandler("onresize",function() {shovelerMain(0,false);},"resized"); } else { N2ChainEventHandler("onresize",function() {shovelerMain(1,false);},"resized"); }

function onAjaxUpdate_purchase_similarities(asin, initialLoad) {
  shovelerMain(1,initialLoad);
}

function onCacheUpdate_purchase_similarities(asin, initialLoad) {
  shovelerMain(1,initialLoad);
}

function onCacheUpdateReselect_purchase_similarities(asin, initialLoad) {
  shovelerMain(1,initialLoad);
}
</script>

</div>
