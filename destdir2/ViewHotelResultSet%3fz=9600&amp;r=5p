<script language="javascript" type="text/javascript">
var isDateless=false;
function adjustPadding(pNum) {

	if(document.getElementById("promo"+pNum)){
		var offset = document.getElementById("promo"+pNum).getElementsByTagName("DIV")[0].offsetHeight;
		if(!is_ie) {offset = offset-20;}
		document.getElementById("priceId"+pNum).getElementsByTagName("DIV")[0].style.paddingBottom=offset+'px';
		document.getElementById("descId"+pNum).getElementsByTagName("DIV")[0].style.paddingBottom=offset+'px';
		if(is_saf) {
			document.getElementById("promo"+pNum).getElementsByTagName("DIV")[0].style.marginTop="5px"; // required for Safari to display properly!
		}
	}
}

</script>

