<script language="javascript" type="text/javascript">
if(parent.document.getElementById("AMENITY")){
	
		
			parent.document.getElementById("AMENITY").style.display="none";
		
		
	
}
	
		resizeFrame();//bw
	

	/* conditional chooser for scroll in iframe */
	var targetDiv = document.getElementById('result');
	if(targetDiv != null) {
		backButtonScroll(getElementPosition(targetDiv).top, true);
	}
</script>


	



	
		
	
	






	


<script type="text/javascript">
	
		
		
			// convert hiaFormData attribute JSON to formHIASubmit() call
			hotelDetailsLinks = $("body").getElementsByAttribute('hiaFormData', 'A');
			for (var i=0; i < hotelDetailsLinks.length; i++) {
				hotelDetailsLinks[i].observe('click', function(event){
					var linkElem = Event.findElement(event, "A");
					var hiaFormData = eval('('+linkElem.getAttribute("hiaFormData")+')');
					hiaFormData = $H(hiaFormData);
					var functionParams = "'"+hiaFormData.remove("formName")+"', ";
					functionParams = functionParams+"'"+hiaFormData.remove("formAction")+"'";
					hiaFormData.each(function(pair) {
						functionParams = functionParams+", '"+pair.key+"|"+pair.value+"'";
					});
					return eval("formHIASubmit("+functionParams+")");
				});
			}
		
	
</script>




<script type="text/javascript">	
	function updateMatrix(){
		parent.selectResultSet('n0s0','auto')
	}
	
	//added to resolve bug 58119
	function checkError(){
		var errorMsg=false;
		var pTag = parent.document.getElementById('errorMsg');
		var warnMsg = parent.document.getElementById('warnMsg');
		if(errorMsg==false){
			if(pTag!=null){
				pTag.className='errorInactive';
			}else if(warnMsg!=null){
				warnMsg.className='errorInactive';
			}
		}else{
			if(pTag!=null){
				pTag.className='error';
			}else if(warnMsg!=null){
				warnMsg.className='alert';
			}
		}
	}
	
	resizeFrame();
	updateMatrix();
	//printVersion();
	checkError();
</script>



	
	
	
		
	
	

