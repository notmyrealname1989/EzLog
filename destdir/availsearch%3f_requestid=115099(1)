		if	(hotelDetail2 != 'null') {
			document.getElementById("hdMainContent").style.display = "none";
		}
		 if (document.getElementById('quickResState')){
			document.getElementById('quickResState').style.visibility = "hidden";
		}

      eval ('procLayer.style.visibility = "visible"');

      if (isNS6){
        scrollTo(0,0);
      }
      else {
        document.body.scrollTop = 0;
      }

      eval ('bgLayer.style.top = "0"');
      eval ('bgLayer.style.height = "' + document.body.scrollHeight + '"');
