	function zeroPad(str,size){
		size++;
		for(var i=0; i <= (size-str.length); i++)
			str = '0' + str;
		return str;
	}

	// Pass in the form object from the pclub login
	function validateAndSubmitLogin(theForm) {
		var errMsg = '';
		var pcnStr = theForm.login.value;
		var pinStr = theForm.password.value;

		if(theForm.login.value == '' || theForm.password.value == '') {
			errMsg = errMsg + ' You must enter either an Email address or a Priority Club number and a PIN.\n';
		}

 		if(pinStr != '') {
			if(pinStr.length < 4)
				errMsg = errMsg + " Priority Club Membership PIN must be 4 digits.\n";
		}

