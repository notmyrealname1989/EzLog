<script language="javascript" type="text/javascript">
  
function populateCalendars (theForm, brand, region, locale, secure)
{
	var cDate = theForm.checkInDate[theForm.checkInDate.selectedIndex].value;
	if(cDate == null || cDate == ""){
	    cDate = "-1";
	}
	var cMonthYear = theForm.checkInMonthYear[theForm.checkInMonthYear.selectedIndex].value;
	var url = '/h/d/' + brand + '/' + region + '/' + locale + '/pop_calendar_panels?test=grb&openerForm=' + theForm.name + '&monthYear=' + cMonthYear + '&date=' + cDate + "1" + '&checkOut=false&secure=' + secure;
	document.getElementById("calendar_pop_in").innerHTML = "<iframe SRC=" + url + " WIDTH=\"266\" HEIGHT=\"133\" scrolling=\"no\" FRAMEBORDER=0 style=\"border: '1px solid'\">Your browser does not support the iframe tag.  Please consider upgrading your browser.</iframe>";	

	var coDate = theForm.checkInDate[theForm.checkOutDate.selectedIndex].value;
	if(coDate == null || coDate == ""){
	    coDate = "-1";
	}

	var coMonthYear = theForm.checkOutMonthYear[theForm.checkOutMonthYear.selectedIndex].value;
	var url2 = '/h/d/' + brand + '/' + region + '/' + locale + '/pop_calendar_panels?test=grb&openerForm=' + theForm.name + '&monthYear=' + coMonthYear + '&date=' + coDate + '&checkOut=true&secure=' + secure;
	document.getElementById("calendar_pop_out").innerHTML = "<iframe SRC=" + url2 + " WIDTH=\"266\" HEIGHT=\"133\" scrolling=\"no\" FRAMEBORDER=0 style=\"border: '1px solid'\">Your browser does not support the iframe tag.  Please consider upgrading your browser.</iframe>";	
	return false;
}

populateCalendars (calendarPopForm, 'hi', '1', 'en', 'false');

function rewriteURLs() {
	document.calendarPopForm.availabilitySearchSuccessURL.value = "/h/d/hi/1/en/rates/" + document.calendarPopForm.hotelCode.value + "?ias=y";
	}
function openPopCalendarCheckIn(){
	document.getElementById("calendar_pop_out").style.display = "none";
	document.getElementById("calendar_pop_in").style.display = "inline";
	}
function openPopCalendarCheckOut() {
	document.getElementById("calendar_pop_in").style.display = "none";
	document.getElementById("calendar_pop_out").style.display = "inline";
	}
</script>









</div>		
			
    

	



    
    
  

    

    
    

	
		
			
            
            
                
            
		

