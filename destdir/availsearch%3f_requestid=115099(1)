  function showProcPage() {
	var hotelDetail2 = "null";
    if (isNS4){
       return;
    }
    else {
      if (isIE4) {
        procLayer = document.all["proc"];
        bgLayer = document.all["bg"];
      }
      else if (isIE5 || isNS6) {
        procLayer = document.getElementById("proc");
        bgLayer = document.getElementById("bg");
      }
      else{
        return;
      }
    }
    if(procLayer){
      document.body.style.visibility = "hidden";
      if(chinapay)
      {
          document.getElementById("waitimageforchinapay").style.display= 'block';
          document.getElementById("waitimageforrest").style.display= 'none';

      }else{
          document.getElementById("waitimageforrest").style.display= 'block';
          document.getElementById("waitimageforchinapay").style.display= 'none';
