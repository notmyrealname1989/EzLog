<script language="javascript">

function averageNightRate(obj){
	positionedPopPanel(obj,'averageNightRate');
	}
function positionedPopPanel(obj,popName) {
	var newX = findPosX(obj);
	var newY = findPosY(obj);
	var x = new getObj(popName);
    <!--  TD #70628 - Start  -->
	x.style.display = 'block';
    <!-- TD #70628 - end    -->
    x.style.top = (newY - 50) + 'px';
	x.style.left = (newX - 244) + 'px';
}
function findPosX(obj) {
	var curleft = 0;
	if (obj.offsetParent) {
		while (obj.offsetParent) {
			curleft += obj.offsetLeft
			obj = obj.offsetParent;
		}
	}
	else if (obj.x)
		curleft += obj.x;
	return curleft;
}
function findPosY(obj) {
	var curtop = 0;
	if (obj.offsetParent) {
		while (obj.offsetParent) {
			curtop += obj.offsetTop
			obj = obj.offsetParent;
		}
	}
	else if (obj.y)
		curtop += obj.y;
	return curtop;
}
function getObj(name) {
 if (document.getElementById) {
	   this.obj = document.getElementById(name);
	   this.style = document.getElementById(name).style;
 }
 else if (document.all) {
	   this.obj = document.all[name];
	   this.style = document.all[name].style;
 }
 else if (document.layers) {
	   this.obj = document.layers[name];
	   this.style = document.layers[name];
  }
}

function showRolloverPopup(url, mktgRateTypeName, divId)
{
  return overlib( OLiframeContent(url, 450, 250, 'if2',1,'auto'), WIDTH,400, FGCOLOR,'white',TEXTPADDING,0,BORDER,2,CAPTIONPADDING, 4, CAPTION, mktgRateTypeName, MIDX,0,MIDY,0,STICKY, CLOSECLICK, REF,divId,REFX,'-300',REFY,'-280',STATUS,'');
}


 function calendarPanelManager(where,which) {
    <!-- move whole form and associated calendars around based on position of button -->
    document.getElementById("calendar_panel").style.top = (where.offsetTop - 15) + "px";
    document.calendarPopForm.hotel_code.value = which;
    cmCreateManualPageviewTag('SELECT_DATES', 'MANUAL_PAGE_VIEW_HOTELSEARCHRESULTS');
    }


</script>


    

