<script language="javascript" type="text/javascript">
  
function populateSideCalendars (theForm, brand, region, locale, secure)
{
	var cDate = theForm.checkInDate[theForm.checkInDate.selectedIndex].value;
	if(cDate == null || cDate == ""){
	    cDate = "-1";
	}
	var cMonthYear = theForm.checkInMonthYear[theForm.checkInMonthYear.selectedIndex].value;
	var url = '/h/d/' + brand + '/' + region + '/' + locale + '/pop_calendar?test=grb&openerForm=' + theForm.name + '&monthYear=' + cMonthYear + '&date=' + cDate + '&checkOut=false&secure=' + secure;
	document.getElementById("calendar_side_pop_in").innerHTML = "<iframe SRC=" + url + " WIDTH=\"213\" HEIGHT=\"215\" scrolling=\"no\" FRAMEBORDER=0 style=\"border: '1px solid'\">Your browser does not support the iframe tag.  Please consider upgrading your browser.</iframe>";	

	var coDate = theForm.checkInDate[theForm.checkOutDate.selectedIndex].value;
	if(coDate == null || coDate == ""){
	    coDate = "-1";
	}

	var coMonthYear = theForm.checkOutMonthYear[theForm.checkOutMonthYear.selectedIndex].value;
	var url2 = '/h/d/' + brand + '/' + region + '/' + locale + '/pop_calendar?test=grb&openerForm=' + theForm.name + '&monthYear=' + coMonthYear + '&date=' + coDate + '&checkOut=true&secure=' + secure;
	document.getElementById("calendar_side_pop_out").innerHTML = "<iframe SRC=" + url2 + " WIDTH=\"213\" HEIGHT=\"215\" scrolling=\"no\" FRAMEBORDER=0 style=\"border: '1px solid'\">Your browser does not support the iframe tag.  Please consider upgrading your browser.</iframe>";	
	return false;
}

populateSideCalendars (searchForm, 'hi', '1', 'en', 'false');

function openSidePopCalendarCheckIn(where){
	document.getElementById("calendar_side_pop_in").style.display = "inline";
	}
function openSidePopCalendarCheckOut(where) {
	document.getElementById("calendar_side_pop_out").style.display = "inline";
	}
</script>








</td>
                                </tr>
                                
                                
