<script language="javascript" type="text/javascript">

function gotStates(value) {
	var ccode;
    if (value){
    		ccode = value;
    		}
    else if(document.getElementById('countryselect')!=null) {
    		ccode = document.getElementById('countryselect').value;
    		}
 	if (ccode == "0001" || ccode == "USA" ) {
		document.getElementById('stateLabel').innerHTML = "State:";
		}
	else if (ccode == "0260" || ccode == "CAN" ) {
		document.getElementById('stateLabel').innerHTML = "Province:";
	}

 	if (ccode == "0001" || ccode == "0260" || ccode == "USA" || ccode == "CAN") {
		document.getElementById('stateSet').style.visibility = "visible";
		return false;
		}
	else if(document.getElementById('stateSet')!=null){
		document.getElementById('stateSet').style.visibility = "hidden";
		return false;
	}
}

function clearDestState(){
		if (document.getElementById('destStateSet')) {
			document.getElementById('destStateSet').style.visibility = "hidden";
			}
	}

function blockin(which){
	var posID = ("positive" + which);
	var negID = ("negative" + which);

if (document.images[negID].style.display)
	{
	show = (document.images[posID].style.display == 'none') ? 'inline' : 'none';
	document.images[posID].style.display = show;
	show = (document.images[posID].style.display == 'none') ? 'inline' : 'none';
	document.images[negID].style.display = show;
	}

	var which_block = new Array("booking_content", "brands_content", "amenities_content", "hotels_along_content", "route_fields");
	sect = (which - 1);
	sect = which_block[sect];

	if (document.layers)
		{
		current = (document.layers[sect].style.display == 'block') ? 'none' : 'block';
		document.layers[sect].style.display = current;
		}
	else if (document.all)
		{
		current = (document.all[sect].style.display == 'block') ? 'none' : 'block';
		document.all[sect].style.display = current;
		}
	else if (document.getElementById)
		{
		vista = (document.getElementById(sect).style.display == 'block') ? 'none' : 'block';
		document.getElementById(sect).style.display = vista;
		}
	footerPosition();
	}

function collapseAll(){
	var which_block = new Array("booking_content", "brands_content", "amenities_content", "hotels_along_content", "route_fields");
	for (var i=0; i<5; i++){
		var item = which_block[i];
		if (document.getElementById(item)){
			if (document.layers)
				{
				if (document.layers[item].style.display == 'block') { document.layers[item].style.display = 'none'; }
				}
			else if (document.all)
				{
				if (document.all[item].style.display == 'block') { document.all[item].style.display = 'none'; }
				}
			else if (document.getElementById)
				{
				if (document.getElementById(item).style.display == 'block') { document.getElementById(item).style.display = 'none'; }
				}
			}

		j = i + 1;
		var posID = ("positive" + j);
		var negID = ("negative" + j);

		if (document.images[negID])
			{
			if (document.images[negID].style.display == 'inline') {
				document.images[posID].style.display = 'inline';
				document.images[negID].style.display = 'none';
				}
			}

		}
	}
function clearCity() {
	document.searchForm.city.value='';
	document.searchForm.stateId.value='';
	document.searchForm.countryId.value='';
	document.getElementById('stateSet').style.visibility= 'hidden';
}

function clearTPI() {
	<!-- temp placeholder -->
	}

function clearFaveDest() {
	if (document.searchForm.favDest) {
		document.searchForm.favDest.value='';
		}
	}
function footerPosition() {
                        var centerHeight = (document.getElementById('advanced_search_form').offsetHeight + document.getElementById('advanced_search_form').offsetTop);
                        if (centerHeight >= 650) {
                          
                          document.getElementById('adv_footer').style.top = ((document.getElementById('advanced_search_form').offsetHeight + document.getElementById('advanced_search_form').offsetTop + 45) + "px");
                          document.getElementById('leftNavInt').style.height = ((document.getElementById('advanced_search_form').offsetHeight + 30) + "px");
                          document.getElementById('rightNavIntColumn').style.height = ((document.getElementById('advanced_search_form').offsetHeight + 160) + "px");
                        } else {
                          
                          document.getElementById('adv_footer').style.top = ((document.getElementById('advanced_search_form').offsetHeight + document.getElementById('advanced_search_form').offsetTop + 45) + "px");
                          document.getElementById('leftNavInt').style.height = "492px";
                          document.getElementById('rightNavIntColumn').style.height = "600px";
                        }
                      }
function resetAll(){
	milesFlag=false;
	document.searchForm.reset();
	var numOfElements = document.searchForm.elements.length;
		for(i=0;i<numOfElements;i++){
			 if(document.searchForm.elements[i].type == 'select-one'){
				if(document.searchForm.elements[i].name=='radius'){
					document.searchForm.elements[i].value=30;
				}else if (document.searchForm.elements[i].name=='number_Nights'){
                    continue;
				}else{
					document.searchForm.elements[i].value=document.searchForm.elements[i].options[0].value;
				}
			 }
			 if(document.searchForm.elements[i].type == 'text'){
				if(document.searchForm.elements[i].name=='checkInFullDate' ||document.searchForm.elements[i].name=='checkOutFullDate'){
				  document.searchForm.elements[i].value='mm/dd/yyyy';
				} else {
				  document.searchForm.elements[i].value='';
				}
			 }

			 if(document.searchForm.elements[i].type == 'radio'){
				if(document.searchForm.elements[i].name=='inMiles' && milesFlag==false){
					document.searchForm.elements[i].checked=true;
					milesFlag=true;
                }
			 }
	   }
 }

</script>


