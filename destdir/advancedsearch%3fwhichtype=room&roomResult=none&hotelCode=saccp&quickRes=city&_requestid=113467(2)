<script>
var selectFlag =-1;
function clearCityTextBox(){					
	var selectBox = document.searchForm.countryId.value;								
	if(selectFlag!=-1){
		document.searchForm.city.value="";
	}
	selectFlag=selectBox;					
	return true;
}
function clearCityTextBoxValue(){
	var selectBox = document.searchForm.countryIds.value;
	if(selectFlag!=-1){
		document.searchForm.city.value="";
	}
	selectFlag=selectBox;
	return true;
}

function setSelectFlag(){
	var selectBox = document.searchForm.countryId.value;					
	if(selectBox=="")	{
		selectFlag=-1;
	} else {
		selectFlag=selectBox;
	}
	return true;
}	
function setSelectFlags(){
	var selectBox = document.searchForm.countryIds.value;
	if(selectBox=="")	{
		selectFlag=-1;
	} else {
		selectFlag=selectBox;
	}
	return true;
}
</script>


