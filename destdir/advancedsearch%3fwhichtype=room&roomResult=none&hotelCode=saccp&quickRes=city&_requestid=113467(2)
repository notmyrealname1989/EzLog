// IF AGENTS ARE AVAILABLE:
function agents_available() {


document.getElementById('smartbutton').innerHTML = '<A HREF="" onClick="window.open(\'http://www.ichotelsgroup.com/h/d/hi/1/en/chatcustomer?hotelName=SACRAMENTO-CAPITOL PLAZA&pageId=advancedsearch\',\'custclient\',\'width=500,height=320,scrollbars=0\');cwaMeasurement();return false;"><img  src="http://images3.ichotelsgroup.com/cq/etc/media_library/hi/0/en/brbt.Par.0002.Image.gif" alt="Click here to start a chat session with a reservations agent"></A>';
if (document.getElementById('i_top_status_faceplateInt')) { document.getElementById('i_top_status_faceplateInt').style.visibility = 'visible'; } 
if (document.getElementById('i_top_status_faceplate_content')) { document.getElementById('i_top_status_faceplate_content').style.visibility = 'visible'; } 

//Next line seems to screw up styles. When removed it works fine. 
//if (document.getElementById('leftNavInt')) { 
//document.getElementById('leftNavInt').style.height = (document.getElementById('rightNavIntColumn').offsetHeight + "px"); }

var hdChatTalkButtons = document.getElementById('hdChatTalkButtons');
if (hdChatTalkButtons) {
   hdChatTalkButtons.style.display='block';
   hdChatTalkButtons.style.visibility='visible';
   hdChatTalkButtons.style.backgroundColor='#FFF';
   hdChatTalkButtons.style.paddingTop='10px';
   hdChatTalkButtons.style.paddingBottom='10px';
}
return true;
}

// IF AGENTS ARE NOT AVAILABLE:
function agents_not_available() {
document.getElementById('smartbutton').innerHTML = '<A HREF="" onClick="window.open(&#039;https://admin.instantservice.com/links/5768/15020&#039;, &#039;custclient&#039;, &#039;width=500,height=320&#039;); emaMeasurement();return false;"><img  src="http://images3.ichotelsgroup.com/cq/etc/media_library/cn/0/en/textimg/phnu.Par.0003.Image.gif" alt="Click here to email a reservations agent" border="0"></A>';
return true;
}
