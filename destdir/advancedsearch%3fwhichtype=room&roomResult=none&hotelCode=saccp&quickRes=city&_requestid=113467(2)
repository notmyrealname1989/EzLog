	   function validateAndSubmit(theForm) {		
		var errMsg = '';
		var pcnStr = theForm.login.value;
		var pinStr = theForm.password.value;
		
		var numInvalid = 0;
		var pinInvalid = 0;
		
		if(theForm.login.value == '' || theForm.password.value == '') {
			errMsg = errMsg + ' You must enter either an Email address or a Priority Club number and a PIN.\n';
			
			if(pcnStr == ''){
			      numInvalid=1;
				}else{
				  pinInvalid=1;
				}
			}
 		if(pinStr != '') {
			if(pinStr.length < 4){
				errMsg = errMsg + " Priority Club Membership PIN must be 4 digits.\n";
				pinInvalid = 1;
                      		}
		}

		if(errMsg != '') {
			//alert(errMsg);			
			var msgdiv;
			
			if(numInvalid == 1){
			  document.pcLogin.login.style.borderColor  = "#FF0000";
			  document.pcLogin.login.style.borderStyle="solid";
			  document.getElementById("arrowPCNum").style.display='block';
			  document.getElementById("arrowPCPin").style.display='none';
			  msgdiv=document.getElementById("clientErrPopup");
			  document.getElementById("clientErrPopup").style.display='block';
			  document.getElementById("clientPinErrPopup").style.display='none';
			}else if(pinInvalid == 1){
			   document.pcLogin.password.style.borderColor  = "#FF0000";
			   document.pcLogin.password.style.borderStyle="solid";	
			   document.getElementById("arrowPCPin").style.display='block';
			   document.getElementById("arrowPCNum").style.display='none';
			   msgdiv=document.getElementById("clientPinErrPopup");
			   document.getElementById("clientErrPopup").style.display='none';
			   document.getElementById("clientPinErrPopup").style.display='block';
			}
			msgdiv.innerHTML = errMsg;
			return false;
		}
		else
		{			
			return true;
		}

	}
	   
	
	// Pass in the form object from the pclub login
	function validateAndSubmitLogin(theForm) {
		var errMsg = '';
		var pcnStr = theForm.login.value;
		var pinStr = theForm.password.value;

		if(theForm.login.value == '' || theForm.password.value == '') {
			errMsg = errMsg + ' You must enter either an Email address or a Priority Club number and a PIN.\n';
		}

 		if(pinStr != '') {
			if(pinStr.length < 4)
				errMsg = errMsg + " Priority Club Membership PIN must be 4 digits.\n";
		}

		if(errMsg != '') {
			alert(errMsg);
			return false;
		}
		else
		{
			return true;
		}

	}

