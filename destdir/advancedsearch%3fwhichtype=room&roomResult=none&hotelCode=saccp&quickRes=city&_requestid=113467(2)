	function zeroPad(str,size){
		size++;
		for(var i=0; i <= (size-str.length); i++)
			str = '0' + str;
		return str;
	}

	function showBackEndError(){
		document.pcLogin.login.style.borderColor  = "#FF0000";
		document.pcLogin.login.style.borderStyle="solid";	
	}

	function closePopUp(theForm){					
		document.pcLogin.login.style.borderColor  = "#FFFFFF";
		document.pcLogin.password.style.borderColor  = "#FFFFFF";
		var backendmsgpopup=document.getElementById("backendErrPopup");
		var arrow = document.getElementById("arrowBackEndError");

