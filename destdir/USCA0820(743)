function rsiConverter(category){
var rsiObj=new Array();var rsiCategory="";
	if(category != undefined && category != ""){
		var rsiStr=category;var rsiSplitUP = rsiStr.split("-");
		for (var i=0; i < rsiSplitUP.length; i++){
			rsiObj[i] = rsiSplitUP[i];
		}
		rsiCategory = rsiObj.join(" > ");rsiCategory = rsiCategory+" > ";
	}
return rsiCategory;
}
