<script language=javascript>

function updateParentAndClose() {
    var reslink = document.getElementById("reservea")
    var agt=navigator.userAgent.toLowerCase();

    // sniff for Safari browser as it needs a more specific DOM
    // reference for accessing the parent window - Defect #34118

    if (agt.indexOf("safari") != -1) {
	    window.opener.window.location.href = reslink.href;
	} else {
	    opener.location.assign(reslink.href);
	}
    window.close()
}
</script>
