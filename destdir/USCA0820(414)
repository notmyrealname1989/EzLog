var mw_fcstArray = new Array();

function mw_propsObj(thumbURL,largeURL,destURL,presName,animDestUrl) {
	this.thumbURL = thumbURL;
	this.largeURL = largeURL;
	this.destURL = destURL;
	this.presName = presName;
	this.animDestUrl = animDestUrl;
}

