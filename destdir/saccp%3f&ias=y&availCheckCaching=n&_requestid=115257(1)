<!--
function ajaxPopup(linkElement, funcName){
	function initXMLHR(url)
	{
	  var requester;
	  try
	  {
	    requester = new XMLHttpRequest();
	  }
	  catch (error)
	  {
	    try
	    {
	      requester = new ActiveXObject("Microsoft.XMLHTTP");
	    }
	    catch (error)
	    {
	      requester = null;
	    }
	  }
	
	  if (requester != null)
	  {
	    requester.onreadystatechange = function()
	    {
	      if (requester.readyState == 4)
	      {
	        if (requester.status == 200 || requester.status == 304)
	        {
	          success(requester);
	        }
	        else
	        {
	          failure(requester);
	        }
	      }
	
	      return true;
	    };
	
	    requester.open("GET", url);
	    requester.send(null);
	  }
	  else
	  {
	    return false;
	  }
	
	  return true;
	}
	
	function success(requester)
	{
	  var linkContent = requester.responseText;	
	  
	  displayPopupWithContent(linkTitle, funcName, linkTitle, linkContent, 550, 300, 'Packages');
	  var popupTitleDiv = $('hdPopupTitle');
	  popupTitleDiv.innerHTML = 'InterContinental Hotels Group';
	
	  return true;
	}
	
	function failure(requester)
	{
	  alert("The XMLHttpRequest failed with status code: " + requester.status);
	
	  return true;
	}
	
	var linkTitle = linkElement.getAttribute("title");
	var linkHREF = linkElement.getAttribute("href");
	
	initXMLHR(linkHREF);
}
function ANRPopup(content, funcName){
	var linkTitle = "AverageNightlyRate";
	var linkContent = content;
	
	displayPopupWithContent(linkTitle, funcName, linkTitle, linkContent, 550, 300, 'Packages');
	var popupTitleDiv = $('hdPopupTitle');
	popupTitleDiv.innerHTML = 'InterContinental Hotels Group';
}
//-->
</script>



	
