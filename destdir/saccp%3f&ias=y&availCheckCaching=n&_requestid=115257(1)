  function showInterstitialPage(brandName,brandCode) {
  var rightLogo = '';
    if (isNS4){
       return;
    }
    else {
      if (isIE4) {
        interstitialProcLayer = document.all["interstitialproc"];
        interstitialBgLayer = document.all["interstitialbg"];
      }
      else if (isIE5 || isNS6) {
        interstitialProcLayer = document.getElementById("interstitialproc");
        interstitialBgLayer = document.getElementById("interstitialbg");

	  }
      else{
        return;
      }
   	}
    if(interstitialProcLayer){
    	var innerHtmlContent = interstitialProcLayer.innerHTML;
		rightLogo = '<img  src="http://images3.ichotelsgroup.com/cq/etc/media_library/cn/0/cn/brand_logo/'+brandCode.toLowerCase()+'.Par.0006.Image.gif" name="img3" height="76" width="120" border="0">';
		innerHtmlContent =innerHtmlContent.replace('$$RIGHTIMG$$',rightLogo);
    	innerHtmlContent = innerHtmlContent.replace('$$BRAND$$',brandName);
    	interstitialProcLayer.innerHTML = innerHtmlContent;
      document.body.style.visibility = "hidden";

      eval ('interstitialProcLayer.style.visibility = "visible"');

      if (isNS6){
        scrollTo(0,0);
      }
      else {
        document.body.scrollTop = 0;
      }

      eval ('interstitialBgLayer.style.top = "0"');
      eval ('interstitialBgLayer.style.height = "' + document.body.scrollHeight + '"');
      eval ('interstitialBgLayer.style.visibility = "visible"');

     }
  }

