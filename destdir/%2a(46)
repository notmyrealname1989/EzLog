function clearText(thefield)
	{
	if (thefield.defaultValue==thefield.value) thefield.value = "";
	}

function isNumeric(val)
	{
	var validChars = "0123456789";
	var isNumber = true;
	var testChar = '';
	for (i = 0; i < val.length && isNumber; i++)
		{
		testChar = val.charAt(i);
		if (validChars.indexOf(testChar) == -1)
			{
			isNumber = false;
			}
		}
	return isNumber;
	}

function checkSearch()
	{
	splitName();
	splitting = true;
	if (document.wsfyForm.firstName.value == "First Name")
		{
		alert("Please enter your First Name");
		document.wsfyForm.firstName.focus();
		return false;
		}
	else if (document.wsfyForm.firstName.value == "")
		{
		if (!splitting) alert("Please enter your First Name");
		else alert("Please enter your Full Name");
		if (!splitting) document.wsfyForm.firstName.focus();
		return false;
		}
	if (document.wsfyForm.lastName.value == "Last Name")
		{
		alert("Please enter your Last Name");
		document.wsfyForm.lastName.focus();
		return false;
		}
	else if (document.wsfyForm.lastName.value == "")
		{
		if (!splitting) alert("Please enter your Last Name");
		else alert("Please enter your Full Name");
		if (!splitting) document.wsfyForm.lastName.focus();
		return false;
		}
	if (document.wsfyForm.age.value == "Age")
		{
		alert("Please enter your Age");
		document.wsfyForm.age.focus();
		return false;
		}
	else if (document.wsfyForm.age.value == "")
		{
		alert("Please enter your Age");
		document.wsfyForm.age.focus();
		return false;
		}
	else if (!isNumeric(document.wsfyForm.age.value))
		{
		alert("Please enter a correct Age");
		document.wsfyForm.age.focus();
		return false;
		}
	}

function splitName()
	{
	var fullName = document.wsfyForm.fullName.value;
	fullName = fullName.split("");
	var lastSpace;
	for (var i=0; i<fullName.length; i++) if (fullName[i] == " " && fullName[i+1] && fullName[i+1] != " ") lastSpace = i;
	var firstName = "";
	for (var i=0; i<lastSpace; i++) firstName += fullName[i];
	var lastName = "";
	for (var i=lastSpace+1; i<fullName.length; i++) lastName += fullName[i];
	document.wsfyForm.firstName.value = firstName;
	document.wsfyForm.lastName.value = lastName;
	}
