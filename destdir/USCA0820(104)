var hasGold=GetCookie('WxGold');
if (hasTicket !=null && typeof(hasTicket)!='undefined' && hasTicket!=""){hasTicket =true;}
if (hasGold!=null && typeof(hasGold)!='undefined' && hasGold!=""){hasGold=true;}
