  </td>
</tr>
<tr><td colspan="3">











<script language="Javascript" type="text/javascript">

var checkboxImages = new Array(
					"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/check-off._V47060180_.gif",
                    "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/check-on._V47060003_.gif",
					"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/check-hover._V47060180_.gif"
		 			  );

var starImages = new Array(
			"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/unrated._V47082131_.gif",
			"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/1-star._V47060850_.gif",
			"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/2-star._V47082458_.gif",
			"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/3-star._V47082143_.gif",
			"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/4-star._V47082023_.gif",
			"http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/ratings-bar/light-blue/5-star._V47060241_.gif"
	);

var starMessages = new Array(
			"Unrated",
			"I hate it",
			"I don't like it",
			"It's OK",
			"I like it",
			"I love it",
			"Rate it",
			"<span style="+'"'+"color: #990000"+'"'+">Saved</span>",
            "&nbsp;");
var ownItMessages = new Array(
			"I own it",
			"<span style="+'"'+"color: #990000"+'"'+">Saved</span>");

var notInterestedMessages = new Array(
			"Not interested",
			"<span style="+'"'+"color: #990000"+'"'+">Saved</span>");

var excludedMessages = new Array(
			"Don't use for recommendations",
			"<span style="+'"'+"color: #990000"+'"'+">Saved</span>",
            "Don't use to make recommendations");

         
var excludedClickstreamMessages = new Array(
			"Remove from Your Browsing History",
			"<span style="+'"'+"color: #990000"+'"'+">Saved</span>",
            "Don't use to make recommendations");

var isGiftMessages = new Array(
			"This was a gift",
			"<span style="+'"'+"color: #990000"+'"'+">Saved</span>",
            "This was a gift");


var starMap = new Array(
		'0,0,12,20',
		'13,0,24,20',
		'25,0,36,20',
		'37,0,48,20',
		'49,0,60,20',
		'61,0,72,20');

var savedRatings = new Array();
var changedRatings = new Array();
var savedNotInterested = new Array();
var savedIsOwned = new Array();
var savedExcluded = new Array();
var savedExcludedClickstream = new Array();
var savedIsGift = new Array();
var starTwinkler = new Array();
var msgTwinkler = new Array();

var isSaved = new Array();

var isRatingsBarChanged = false;


var delayTime = 500;


var allImages = new Array();

function amz_js_preloadImages(imgArray){
  for (i=0; i < imgArray.length ;i++){
    if (imgArray[i] != undefined) {
      allImages[i] = new Image();
      allImages[i].src = imgArray[i];
    }
  }
}

amz_js_preloadImages(starImages);
amz_js_preloadImages(checkboxImages);


function amz_js_isOwnedMouseOver(asin){
	if (!savedIsOwned[asin]) {
		document.images["isOwned." + asin].src = checkboxImages[2];
	}
}

function amz_js_isOwnedMouseOut(asin){
	if (!savedIsOwned[asin]) {
		document.images["isOwned." + asin].src = checkboxImages[0];
	}
}

function amz_js_notInterestedMouseOver(asin){
	if (!savedNotInterested[asin]) {
		document.images["notInterested." + asin].src = checkboxImages[2];
	}
}

function amz_js_notInterestedMouseOut(asin){
	if (!savedNotInterested[asin]) {
		document.images["notInterested." + asin].src = checkboxImages[0];
	}
}

function amz_js_excludedMouseOver(asin, alt, showNeg){
	if (document.images["excluded." + asin].src == checkboxImages[0]) {
		document.images["excluded." + asin].src = checkboxImages[2];
	}
}

function amz_js_excludedMouseOut(asin, alt,  showNeg){
	if (document.images["excluded." + asin].src == checkboxImages[2]) {
		document.images["excluded." + asin].src = checkboxImages[0];
	}
}

function amz_js_excludedClickstreamMouseOver(asin){
        if (savedExcludedClickstream[asin]) {
                document.images["excludedClickstream." + asin].src = checkboxImages[2];
        }
}

function amz_js_excludedClickstreamMouseOut(asin){
        if (savedExcludedClickstream[asin]) {
                document.images["excludedClickstream." + asin].src = checkboxImages[0];
        }
}


function amz_js_isGiftMouseOver(asin){
        if (!savedIsGift[asin]) {
                document.images["isGift." + asin].src = checkboxImages[2];
        }
}

function amz_js_isGiftMouseOut(asin){
        if (!savedIsGift[asin]) {
                document.images["isGift." + asin].src = checkboxImages[0];
        }
}



function amz_js_sendRating(asin, ratingType, ratingValue){
  


  var submitURL = '/gp/yourstore/ratings/submit.html?'
      + 'session-id=002-7818855-9196047'
      + '&rating.source=ir'
      + '&rating_asin=' + asin
      + '&' + asin + '.rating.' + ratingType + '=' + ratingValue 
	  + '&' + 'ref=pd_rate_dp'
	  + '&template-name=' 
      + '%2Fgp%2Fyourstore%3Fie%3DUTF8%26HASH%25280x6ae36968%2529%3D';


    if ( ( ( ratingType == 'onetofive' ) || ( ratingType == 'owned'  ) )
            && ( savedNotInterested[asin] == 1 ) ) {
        submitURL = submitURL + '&' + asin + '.rating.not-interested=NONE';
        savedNotInterested[asin] = 0;
    }
    else if ( ratingType == 'not-interested' ) {
        if ( savedRatings[asin] > 0 ) {
            submitURL = submitURL + '&' + asin + '.rating.onetofive=0';
            savedRatings[asin] = 0;
        }
        if ( savedIsOwned[asin] == 1 ) {
            submitURL = submitURL + '&' + asin + '.rating.owned=NONE';
            savedIsOwned[asin] = 0;
        }
    }

    try {
        if (shvl != undefined) {
            invalidateAsin(asin);
        }
    }
    catch(e) { }


    isRatingsBarChanged = true;
    window.location.href = submitURL + "&return.response=204";
}


function amz_js_swapStars(asin, rating) {
    if (document.images["stars." + asin] != undefined) {
        if (rating == undefined) rating = savedRatings[asin];
        if (rating == undefined) rating = 0;
        document.images["stars." + asin].src = starImages[rating];
    }  
}

function amz_js_swapStarMsgs(asin, rating) {
    if (!isSaved[asin] && document.getElementById("messages." + asin)) {
        if (rating == undefined) {
            document.getElementById("messages." + asin).innerHTML = starMessages[6];
        } else {
            document.getElementById("messages." + asin).innerHTML = starMessages[rating];
            if (rating == 7) isSaved[asin]= 1;
        }
    }
}

function amz_js_swapIsOwnedMsgs(asin, isOwned) {
	document.getElementById("ownIt.messages." + asin).innerHTML = ownItMessages[isOwned];
}

function amz_js_swapIsOwned(asin, isOwned) {
    if (document.images["isOwned."+asin]) document.images["isOwned." + asin].src = checkboxImages[isOwned];
}

function amz_js_swapNotInterestedMsgs(asin, notInterested) {
	document.getElementById("notInterested.messages." + asin).innerHTML = notInterestedMessages[notInterested];
}

function amz_js_swapNotInterested(asin, notInterested) {
    if (document.images["notInterested."+asin]) document.images["notInterested." + asin].src = checkboxImages[notInterested];
}

function amz_js_swapExcludedMsgs(asin, excluded) {
	document.getElementById("excluded.messages." + asin).innerHTML = excludedMessages[excluded];
}
function amz_js_swapExcludedClickstreamMsgs(asin, excluded) {
        document.getElementById("excludedClickstream.messages." + asin).innerHTML = excludedClickstreamMessages[excluded];
}

function amz_js_swapExcluded(asin, excluded, showNeg) {
    var excVal = (excluded == 1 && showNeg == 0) || (excluded == 0 && showNeg == 1) ? 1 : 0;
    if (document.images["excluded."+asin]) document.images["excluded." + asin].src = checkboxImages[excVal];
}

function amz_js_swapExcludedClickstream(asin, excluded) {
    excluded = excluded ? 0 : 1;
    if (document.images["excludedClickstream."+asin]) document.images["excludedClickstream." + asin].src = checkboxImages[excluded];
}

function amz_js_swapIsGift(asin, isGift) {
    if (document.images["isGift."+asin]) document.images["isGift." + asin].src = checkboxImages[isGift];
}



function amz_js_sendStars(asin, rating) {
    savedRatings[asin] = rating;
    changedRatings[asin] = 1;
    try {
        if (shvl) {
            shvl.rating[asin] = savedRatings[asin];
            shvl.notInterested[asin] = 0;
        }    
    } catch(e) {}     
    amz_js_swapStarMsgs(asin, 7);
    if (savedRatings[asin]) {
        amz_js_swapNotInterested(asin, 0);
    }
    window.setTimeout("amz_js_sendRating('"+asin+"', 'onetofive', '"+rating+"')", delayTime);
    
}

function amz_js_sendIsOwned(asin) {
    savedIsOwned[asin] = (savedIsOwned[asin] ? 0 : 1);
    rating = (savedIsOwned[asin] ? 'OWN' : 'NONE');
    try {
        if (shvl) {
            shvl.isOwned[asin] = savedIsOwned[asin];
            shvl.notInterested[asin] = 0;
        }    
    } catch(e) {}     
    amz_js_swapIsOwned(asin,savedIsOwned[asin]);
    if (savedIsOwned[asin]) {
        amz_js_swapNotInterested(asin, 0);
    }
    amz_js_swapStarMsgs(asin, 7);
    window.setTimeout("amz_js_sendRating('"+asin+"', 'owned', '"+rating+"')", delayTime);
}

function amz_js_sendNotInterested(asin) {
    savedNotInterested[asin] = (savedNotInterested[asin] ? 0 : 1);
    try {
        if (shvl) {
            shvl.notInterested[asin] = savedNotInterested[asin];
            shvl.isOwned[asin] = 0;
            shvl.rating[asin] = 0;
        }    
    } catch(e) {}    
        
    rating = (savedNotInterested[asin] ? 'NOTINTERESTED' : 'NONE');
    amz_js_swapNotInterested(asin,savedNotInterested[asin]);
    if (savedNotInterested[asin]) {
        amz_js_swapIsOwned(asin, 0); 
        amz_js_swapStars(asin, 0);
    }
    amz_js_swapStarMsgs(asin, 7);
    window.setTimeout("amz_js_sendRating('"+asin+"', 'not-interested', '"+rating+"')", delayTime);
}

function amz_js_sendExcluded(asin, msg, showNeg) {
    savedExcluded[asin] = (savedExcluded[asin] ? 0 : 1);
    rating = (savedExcluded[asin] ? 'EXCLUDED' : 'NONE');
    amz_js_swapExcluded(asin,savedExcluded[asin],showNeg);
    amz_js_swapStarMsgs(asin, 7);
    window.setTimeout("amz_js_sendRating('"+asin+"', 'excluded', '"+rating+"')", delayTime);
}

function amz_js_sendExcludedClickstream(asin) {
    savedExcludedClickstream[asin] = (savedExcludedClickstream[asin] ? 0 : 1);
    rating = (savedExcludedClickstream[asin] ? 'NONE' : 'EXCLUDED');
    amz_js_swapExcludedClickstream(asin,savedExcludedClickstream[asin]);
    amz_js_swapStarMsgs(asin, 7);
    window.setTimeout("amz_js_sendRating('"+asin+"', 'excludedClickstream', '"+rating+"')", delayTime);
}


function amz_js_sendIsGift(asin, msg) {
    savedIsGift[asin] = (savedIsGift[asin] ? 0 : 1);
    var giftVal = (savedIsGift[asin] ? 'ISGIFT' : 'NONE');
    amz_js_swapIsGift(asin,savedIsGift[asin]);
    amz_js_swapStarMsgs(asin, 7);
    window.setTimeout("amz_js_sendRating('"+asin+"', 'isGift', '"+giftVal+"')", delayTime);
}




function amz_js_starMouseOver(asin, rating){
   if (starTwinkler[asin] != 0){
    window.clearTimeout(starTwinkler[asin]);
    starTwinkler[asin] = 0;
  }
  if (msgTwinkler[asin] != 0){
    window.clearTimeout(msgTwinkler[asin]);
    msgTwinkler[asin] = 0;
  }  
  amz_js_swapStars(asin, rating);
  amz_js_swapStarMsgs(asin, rating);
}

function amz_js_starMouseOut(asin){
  starTwinkler[asin] = window.setTimeout("amz_js_swapStars('"+asin+"')", delayTime);
  msgTwinkler[asin] = window.setTimeout("amz_js_swapStarMsgs('"+asin+"')", delayTime);
}


function amz_js_showIsOwned(asin, isOwned){
    var imageID = "isOwned." + asin;
    document.write("<a href="+'"#"'+" onclick="+'"'+"amz_js_sendIsOwned('" + asin + "', 'alt'); return false;" + '" ');
    document.write("onfocus="+'"'+"amz_js_isOwnedMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onblur="+'"'+"amz_js_isOwnedMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write('>');
    document.write("<img tabindex="+'"0" '+"src="+'"'+"" + checkboxImages[isOwned] + ""+'"'+"");
    document.write('alt="' + ownItMessages[0] + '" ');
    document.write("id="+'"'+"" + imageID + ""+'"'+" ");
    document.write("onmouseover="+'"'+"amz_js_isOwnedMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_isOwnedMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("border="+'"'+"0"+'"'+" valign="+'"'+"bottom"+'"'+" />");
    document.write("</a>");
}


function amz_js_showNotInterested(asin, notInterested){
    var imageID = "notInterested." + asin;
    document.write("<a href="+'"#"'+" onclick="+'"'+"amz_js_sendNotInterested('" + asin + "', 'alt'); return false;" + '" ');
    document.write("onfocus="+'"'+"amz_js_notInterestedMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onblur="+'"'+"amz_js_notInterestedMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write('>');
    document.write("<img tabindex="+'"0" '+"src="+'"'+"" + checkboxImages[notInterested] + ""+'"'+"");
    document.write('alt="' + notInterestedMessages[0] + '" ');
    document.write("onmouseover="+'"'+"amz_js_notInterestedMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_notInterestedMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("id="+'"'+"" + imageID + ""+'"'+" ");
    document.write("border="+'"'+"0"+'"'+" valign="+'"'+"bottom"+'"'+" />");
    document.write("</a>");
}

function amz_js_showExcluded(asin, excluded, showNeg){
    showNeg = showNeg ? showNeg : 0;
    var imageID = "excluded." + asin;
    var excVal = (excluded == 1 && showNeg == 0) || (excluded == 0 && showNeg == 1) ? 1 : 0;
    document.write("<a href="+'"#"'+" onclick="+'"'+"amz_js_sendExcluded('" + asin + "', 'alt', '" + showNeg +"'); return false;" + '" ');
    document.write("onfocus="+'"'+"amz_js_excludedMouseOver('" + asin + "', 'alt', '" + showNeg +"');"+'"'+" ");
    document.write("onblur="+'"'+"amz_js_excludedMouseOut('" + asin + "', 'alt', '" + showNeg +"');"+'"'+" ");
    document.write('>');
    document.write("<img tabindex="+'"0" '+"src="+'"'+"" + checkboxImages[excVal] + ""+'"'+"");
    document.write('alt="' + excludedMessages[0] + '" ');
    document.write("onmouseover="+'"'+"amz_js_excludedMouseOver('" + asin + "', 'alt', '" + showNeg +"');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_excludedMouseOut('" + asin + "', 'alt', '" + showNeg +"');"+'"'+" ");
    document.write("id="+'"'+"" + imageID + ""+'"'+" ");
    document.write("border="+'"'+"0"+'"'+" valign="+'"'+"bottom"+'"'+" />");
    document.write("</a>");
}

function amz_js_showExcludedClickstream(asin, excluded){
    var imageID = "excludedClickstream." + asin;
    var excVal = excluded ? 0 : 1;
    document.write("<a href="+'"#"'+" onclick="+'"'+"amz_js_sendExcludedClickstream('" + asin + "', 'alt'); return false;" + '" ');
    document.write("onfocus="+'"'+"amz_js_excludedClickstreamMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onblur="+'"'+"amz_js_excludedClickstreamMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write('>');
    document.write("<img tabindex="+'"0" '+"src="+'"'+"" + checkboxImages[excVal] + ""+'"'+"");
    document.write('alt="' + excludedClickstreamMessages[0] + '" ');
    document.write("onmouseover="+'"'+"amz_js_excludedClickstreamMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_excludedClickstreamMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("id="+'"'+"" + imageID + ""+'"'+" ");
    document.write("border="+'"'+"0"+'"'+" valign="+'"'+"bottom"+'"'+" />");
    document.write("</a>");
}

function amz_js_showIsGift(asin, isGift){
    var imageID = "isGift." + asin;
    document.write("<a href="+'"#"'+" onclick="+'"'+"amz_js_sendIsGift('" + asin + "', 'alt'); return false;" + '" ');
    document.write("onfocus="+'"'+"amz_js_isGiftMouseOver('" + asin + "', 'alt');" + '"'+" ");
    document.write("onblur="+'"'+"amz_js_isGiftMouseOut('" + asin + "', 'alt');" + '"'+" ");
    document.write('>');
    document.write("<img src="+'"'+"" + checkboxImages[isGift] + ""+'"'+"");
    document.write('alt="' + isGiftMessages[0] + '" ');
    document.write("onmouseover="+'"'+"amz_js_isGiftMouseOver('" + asin + "', 'alt');" + '"'+" ");
    document.write("onmouseout="+'"'+"amz_js_isGiftMouseOut('" + asin + "', 'alt');" + '"'+" ");
    document.write("id="+'"'+"" + imageID + ""+'"'+" ");
    document.write("border="+'"'+"0"+'"'+" valign="+'"'+"bottom"+'"'+" />");
    document.write("</a>");
}




function amz_js_showStars(asin, rating){
    var starID = "stars." + asin;

    starTwinkler[asin] = 0;
    msgTwinkler[asin] = 0;

    document.write('<map style="cursor: pointer;cursor: hand;" name="starmap' + asin + '">');

    var i = 0;
    for (i = 0; i < 6; i++) {
        document.write('<area shape="rect" coords="' + starMap[i] + '" ' +
            'onmouseover="amz_js_starMouseOver(' + "'" + asin + "'," + i + ');" ' +
            'onfocus="amz_js_starMouseOver(' + "'" + asin + "'," + i + ');" ' +
            'onmouseout="amz_js_starMouseOut(' + "'" + asin + "'" + ');" ' +
            'onblur="amz_js_starMouseOut(' + "'" + asin + "'" + ');" ' +
            'onclick="amz_js_sendStars(' + "'" + asin + "'," + i + '); return false;" ' + 
            'alt="'+starMessages[i]+'" '+
            'href="#" />');
    }
    document.write("</map>");
    document.write('<img src="' + starImages[rating] + '"');
    document.write(' valign="bottom" border="0" usemap="#starmap' + asin + '"');
    document.write(' id="' + starID + '" />');
}

function amz_js_showMessages(asin, defaultMsg){
    if (defaultMsg) {
        starMessages[6] = starMessages[defaultMsg];
    }
    document.write("<span id="+'"'+"messages." + asin + ""+'"'+" class="+'"'+"tiny"+'"'+">" + starMessages[6]  + "</span>");
}

function amz_js_showIsOwnedMessages(asin){
    document.write("<span style="+'"'+"cursor: pointer;cursor: hand;"+'"'+" id="+'"'+"ownIt.messages." + asin + ""+'"'+" ");
    document.write("onclick="+'"'+"amz_js_sendIsOwned('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseover="+'"'+"amz_js_isOwnedMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_isOwnedMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("class="+'"'+"tiny"+'"'+">" + ownItMessages[0] + "</span>");
}

function amz_js_showNotInterestedMessages(asin){
    document.write("<span style="+'"'+"cursor: pointer;cursor: hand;"+'"'+" id="+'"'+"notInterested.messages." + asin + ""+'"'+" ");
    document.write("onclick="+'"'+"amz_js_sendNotInterested('" + asin + "', 'alt'); "+'"'+"");
    document.write("onmouseover="+'"'+"amz_js_notInterestedMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_notInterestedMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("class="+'"'+"tiny"+'"'+">" + notInterestedMessages[0] + "</span>");
}

function amz_js_showExcludedMessages(asin, msg, showNeg){
    msg = msg ? msg : 0;
    showNeg = showNeg ? showNeg : 0;
    document.write("<span style="+'"'+"cursor: pointer;cursor: hand;"+'"'+" id="+'"'+"excluded.messages." + asin + ""+'"'+" ");
    document.write("onclick="+'"'+"amz_js_sendExcluded('" + asin + "', 'alt', '" + showNeg +"'); "+'"'+"");
    document.write("onmouseover="+'"'+"amz_js_excludedMouseOver('" + asin + "', 'alt', '" + showNeg + "');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_excludedMouseOut('" + asin + "', 'alt', '" + showNeg + "');"+'"'+" ");
    document.write("class="+'"'+"tiny"+'"'+">" + excludedMessages[msg] + "</span>");
}

function amz_js_showExcludedClickstreamMessages(asin){
    document.write("<span style="+'"'+"cursor: pointer;cursor: hand;"+'"'+" id="+'"'+"excludedClickstream.messages." + asin + ""+'"'+" ");
    document.write("onclick="+'"'+"amz_js_sendExcludedClickstream('" + asin + "', 'alt'); "+'"'+"");
    document.write("onmouseover="+'"'+"amz_js_excludedClickstreamMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_excludedClickstreamMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("class="+'"'+"tiny"+'"'+">" + excludedClickstreamMessages[0] + "</span>");
}

function amz_js_showIsGiftMessages(asin){
    document.write("<span style="+'"'+"cursor: pointer;cursor: hand;"+'"'+" id="+'"'+"isGift.messages." + asin + ""+'"'+" ");
    document.write("onclick="+'"'+"amz_js_sendIsGift('" + asin + "', 'alt'); "+'"'+"");
    document.write("onmouseover="+'"'+"amz_js_isGiftMouseOver('" + asin + "', 'alt');"+'"'+" ");
    document.write("onmouseout="+'"'+"amz_js_isGiftMouseOut('" + asin + "', 'alt');"+'"'+" ");
    document.write("class="+'"'+"tiny"+'"'+">" + isGiftMessages[0] + "</span>");
}


</script>

