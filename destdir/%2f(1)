        <form style="margin:0px;" method="get" name="registrysearch" action="/gp/registry/search.html/ref=cm_gift_gno_wlsearch">
          <fieldset style="border: 0px; padding: 0px; margin: 0px;">
            <legend style="padding:5px;">
              <label for="gcTopNavSearchFieldName"><span class="gc-title">Find&nbsp;someone's:</span></label><br />
            </legend>
            <div class="gc-radio">
              <input id="gcTopNavSearchWishlist" type="radio" name="type" value="wishlist" checked onClick="document.registrysearch._po_type.value='wishlist';changeNameFieldText(0);"><label for="gcTopNavSearchWishlist">Wish&nbsp;List</label>
              <input id="gcTopNavSearchWedding" type="radio" name="type" value="wedding" onClick="document.registrysearch._po_type.value='wedding';changeNameFieldText(1);"><label for="gcTopNavSearchWedding">Wedding&nbsp;Registry</label>
              <input id="gcTopNavSearchBaby" type="radio" name="type" value="baby" onClick="document.registrysearch._po_type.value='baby';changeNameFieldText(1);"><label for="gcTopNavSearchBaby">Baby&nbsp;Registry</label>
            </div>
	  </fieldset>
	  <div valign="center" align="left" width="100%" style="position:relative;padding:5px;">
	    <input type="hidden" name="field-lastname" value="">
