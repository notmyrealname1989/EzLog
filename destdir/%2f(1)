<script language="javascript" type="text/javascript">
n2RunThisWhen("utilitiesloaded", function() {
  window.a9search = new Object;
  a9search.defText = "Web Search";
  a9search.loadJs = function() {
    if (a9search.scriptLoaded) return false;
    if (a9search.scriptLoading) return true;
    a9search.scriptLoading = true;
    var se = document.createElement("script");
    se.id="a9searchBaseScript";
    se.type="text/javascript";
    se.language="javascript";
    se.src=document.location.protocol+'//a9.com'+'/-/static/scripts/en_US/'+'amzn'+'Srch.js'+"?d="+new Date().getMonth()+new Date().getDate();
    document.body.insertBefore(se, document.body.firstChild);
    return true;
  }
  a9search.fallBack = function( reason ) {
    var s = a9search.searchTerm;
    if (s) {
      if (s == "robots.txt" || s == "favicon.ico") s = "%22" + s + "%22";
      document.location.href = "http://a9.com/"+s+"?dns=www&src=amz";
    }
  }
  a9search.submit = function(txt) {
    if (typeof(txt) == "string") {
        a9search.searchLocation = txt;
        txt = document.getElementById(txt);
    }
    var value = txt.value;
    if (value && value != a9search.defText) {
      if (typeof(encodeURIComponent) != "undefined") {
        value = encodeURIComponent(value);
      } else {
        value = escape(value);
      }
      a9search.searchTerm = value;
      if ( a9search.cacheBox && a9search.cacheBox.value )
        a9search.cacheBox.value = null;
      a9search.page = 1;
      if (a9search.loadJs()) {
        setTimeout(a9search.checkBaseLoad, 15000);
      } else {
        a9search.search();
      }
    } else {
        a9search.showSmpl();
    }
    return false;
  }
  a9search.showSmpl = function() {
      if ( a9search.loadJs() ) {
          setTimeout(a9search.chkSmplLd, 500);
      }
      else {
          a9search.showSmplDiv();
      }
  }
  a9search.chkSmplLdCt = 0;
  a9search.chkSmplLd = function() {
      var scpElem = document.getElementById("a9searchBaseScript");
      if ( scpElem && a9search.showSmplDiv ) {
          a9search.showSmplDiv();
      }
      else if ( a9search.chkSmplLdCt > 6 ) {
          document.location.href = "/gp/redirect.html?%5Fencoding=UTF8&location=http%3A%2F%2Fa9.com%3Fsrc%3Damz%26dns%3Dwww&token=AA2B4D6888E513CDF7C22811766DED1A3F07D572";
      }
      else {
          a9search.chkSmplLdCt++;
          setTimeout(a9search.chkSmplLd, 500);
      }
  }
  a9search.log = function(msg) {
  }
  a9search.checkBaseLoad = function() {
      if (! a9search.scriptLoaded ){
          if ( document.getElementById("a9searchBaseScript") != null ) {
              if ( !a9search.firstChanceBlown )
              {
                  setTimeout(a9search.checkBaseLoad, 15000);
                  a9search.firstChanceBlown = true;
              }
              else
              {
                  a9search.fallBack("Checkbase loaded failed second chance.");
              }
          }
          else {
              setTimeout(a9search.checkBaseLoad, 15000);
          }
      }
  }
  a9search.submitKey = function(evt,txt) {
    evt = evt ? evt : event;
    if (evt.keyCode == 13) a9search.submit(txt);
  }
  a9search.onload = function() {
    if (!a9search.noInitBlur) a9search.blur();
    a9search.popData = [""];
    a9search.pop = new N2SimplePopover();
    goN2Events.registerFeature("a9search", "a9search.pop", null, null, null);
    a9search.pop.initialize("a9searchPop", "a9search.pop", "a9search.popData");
    a9search.pop.locateAt("right");
    a9search.cacheBox = document.getElementById("a9cacheBox");
    if (a9search.cacheBox && a9search.cacheBox.value) {
      var returnVal = a9search.loadJs();
    }
  }
  a9search.blur = function() {
    var t = document.getElementById("a9searchTerm");
    if (t.value == "" || t.value == a9search.defText) {
      t.style.color = "#aaa";
      t.value = a9search.defText;
    }
  }
  a9search.backChecked = false;
  a9search.checkBackButton = function() {
    try
    {
      var t = document.getElementById("a9searchTerm");
      if (t.value != "" && t.value != a9search.defText) {
        if ( a9search.pop != null ) {
          var div = document.getElementById("a9searchPop_main");
          if ( div == null )
          {
            div = document.getElementById("a9searchPop_OuterX");
            if (( div != null ) && ( div.firstChild != null ))
            {
                div = div.firstChild.firstChild;
            }
          }
          if (( div != null ) && ( div.firstChild != null )) {
            if ( div.firstChild.nodeName == "#text" ) {
              t.style.color = "";
              a9search.onload();
            }
          }
        }
      }
      else if ( !a9search.backChecked )
      {
        a9search.backChecked = true;
        setTimeout(a9search.checkBackButton, 2000);
      }
    }
    catch (ex) {
    }
  }
  a9search.focus = function() {
    a9search.noInitBlur = true;
    var t = document.getElementById("a9searchTerm");
    if (t.value == a9search.defText) t.value = "";
    t.style.color = "";
  }
  n2RunThisWhen("lastLibraryLoaded", a9search.onload, "a9search onload");
  n2RunThisWhen("onload", a9search.checkBackButton, "a9search onload");
}, "define a9search after utilities load");
