<script language="JavaScript" type="text/javascript">
oGRPop = false;
function grPopKiller() {
	if (oGRPop) {
		oGRPop.close();
	} else if (parent.oGRPop) {
		parent.oGRPop.close();
	}
}
addEvent(window, "unload", grPopKiller, false);
</script>
	



	
	
		
	




