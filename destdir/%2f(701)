var cnnLocalStorage = '';
Event.observe(window, 'load', function()
{
	cnnLocalStorage = StorageManager.getInstance().getStorage();
		cnnLocalStorage.setOnload( function()
		{
			MainLocalObj.loadDefaultData()
		});
		cnnLocalStorage.load();
});

