<script>
function goToBeach() {

  if (document.boatForm.where.value == "" ) {
    alert("Enter zip code or city.");
    return false;
  } else if (document.boatForm.where.value == "Pacific Grove, CA") {
	window.location="/outlook/recreation/boatandbeach/weather/tenday/USCA0820?from=36hr_outlet_boat";
	return false;
  } else  {
    document.boatForm.lswe.value = document.boatForm.where.value;
    document.boatForm.lswa.value = document.boatForm.what.value;
    return true;
  }
}

function goToVactionPlanner() {
  window.location="/outlook/travel/vacationplanner/destination/weather/USCA0820?lswe=Pacific Grove&lswa=VacationForecastCommand&from=v36_outlet_vacation";
}
</script>

