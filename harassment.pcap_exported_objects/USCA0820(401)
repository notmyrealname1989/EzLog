<script language="JavaScript1.2">
	var interactiveMapRadZoom = 8;
	var interactiveMapSatZoom = 5;
	var activeMapLayer = "radar";
	var activeMapZoom = interactiveMapRadZoom;
	var activeCurrentTabMapIndx = null;
	
	var activeForecastTabDayIndx = 0;
	var activeForecastTabMapTypeIndx = 0;
	var isGold = false;
	
	function showCurrentTabMapfor(Id){
		setCurrentThumbnailStyle(Id);
		if(Id < 2) {
			mw_showHide("mw_mapInMotionLink","block");
			mw_showHide("interactiveMap","block");
			mw_showHide("currentTabClassicMap","none");
			if(activeCurrentTabMapIndx == Id) return;
			if(Id == 0) {showInteractiveGinsuMap("radar", interactiveMapRadZoom);activeMapLayer = "radar";}
			if(Id == 1) {showInteractiveGinsuMap("ussat", interactiveMapSatZoom);activeMapLayer = "ussat";}// the interactive map page is only handling sat param.
			
		}else {
			mw_showHide("mw_mapInMotionLink","none");
			mw_showHide("interactiveMap","none");
			mw_showHide("currentTabClassicMap","block");
			if(activeCurrentTabMapIndx == Id) return;
			imgReplace('classicMapImg',mw_currArray[Id].largeURL);
		}
		activeCurrentTabMapIndx = Id;
	}
	function setCurrentThumbnailStyle(id){
		for(var i=0; i<4; i++){
			if(i==id){
				document.getElementById('CTT'+id).style.cursor='';
				document.getElementById('CTL'+id).style.cursor='';
				document.getElementById('CTT'+id).style.color = '';
				document.getElementById('CTL'+id).style.textDecoration = "";
			}else{
				document.getElementById('CTT'+i).style.cursor='pointer';
				document.getElementById('CTL'+i).style.cursor='pointer';
				document.getElementById('CTT'+i).style.color = '#3333FF';
				document.getElementById('CTL'+i).style.textDecoration = "underline";
			}
		}
	}	
	
	function handleCurrentTabMapClick(){
		if(activeCurrentTabMapIndx < 2) {/*this is handled by goToInteractiveMapPage() function*/}
		else {	window.location = mw_currArray[activeCurrentTabMapIndx].destURL  + "&from=36hour_map_large";	}
	}
	function handlClassicLinkClick() {
		var moreLocalMapsLinkURL = "/outlook/travel/businesstraveler/map/" + "USCA0820";
		if(moreLocalMapsLinkURL.indexOf("?") != -1) {
			// remove the extra parameter as additional parameters are taking
			// users to non-default (first) large map.  More local maps link
			// should always take users to the first map in the collection
			moreLocalMapsLinkURL = moreLocalMapsLinkURL.substring(0, moreLocalMapsLinkURL.indexOf("?"));
		}
		moreLocalMapsLinkURL = moreLocalMapsLinkURL + "?from=36hour_map";
		window.location = moreLocalMapsLinkURL;
	}	
	function activateCurrentTab(){
		deActivateForecastTab();
		mw_showHide("currentTab","block");		
		mw_showHide("currentTabPromoLinks","block");			
		mw_showHide("currentTabThumnail","block");
		showCurrentTabMapfor(activeCurrentTabMapIndx);
	}
	function deActivateCurrentTab(){
		mw_showHide("currentTab","none");
		mw_showHide("currentTabPromoLinks","none");	
		mw_showHide("currentTabThumnail","none");
		mw_showHide("interactiveMap","none");
		mw_showHide("currentTabClassicMap","none");
	}
	
	// Forecast
	function setForecastThumbnailStyle(id){
		for(var i=0; i<4; i++){
			if(i==id){
				document.getElementById('FTT'+id).style.cursor='';
				document.getElementById('FTL'+id).style.cursor='';
				document.getElementById('FTT'+id).style.color = '';
				document.getElementById('FTL'+id).style.textDecoration = "";
			}else{
				document.getElementById('FTT'+i).style.cursor='pointer';
				document.getElementById('FTL'+i).style.cursor='pointer';
				document.getElementById('FTT'+i).style.color = '#3333FF';
				document.getElementById('FTL'+i).style.textDecoration = "underline";
			}
		}
	}
	
	function handleForecastTabMapClick(){		
		window.location = mw_fcstArray[activeForecastTabDayIndx][activeForecastTabMapTypeIndx].destURL + "&from=36hour_map_large";
	}
	function showForecastTabDayMapfor(day){
		activeForecastTabDayIndx = day;
		activateForecastDailyTab(day);
		imgReplace('forecastTabMapImg',mw_fcstArray[activeForecastTabDayIndx][activeForecastTabMapTypeIndx].largeURL);
	}
	function showForecastTabMapTypefor(type){		
		setForecastThumbnailStyle(type);
		logForecastTabThumbnailClick(type);
		activeForecastTabMapTypeIndx = type;
		imgReplace('forecastTabMapImg',mw_fcstArray[activeForecastTabDayIndx][activeForecastTabMapTypeIndx].largeURL);
	}
	function logForecastTabThumbnailClick(type){
		// Forceast tab thumbnail clicks
		if (activeForecastTabDayIndx == 0){
			if (type == 0) actionLib(14);
			else if (type == 1) actionLib(15);
			else if (type == 2) actionLib(16);
			else actionLib(17);
		}else if (activeForecastTabDayIndx == 1){
			if (type == 0) actionLib(18);
			else if (type == 1) actionLib(19);
			else if (type == 2) actionLib(20);
			else actionLib(21);
		}else{
			if (type == 0) actionLib(22);
			else if (type == 1) actionLib(23);
			else if (type == 2) actionLib(24);
			else actionLib(25);
		}
	}
	function activateForecastTab(){
		deActivateCurrentTab();
		mw_showHide("fcstTab","block");
		activateForecastDailyTab(activeForecastTabDayIndx);
		mw_showHide("forecastTabThumnail","block");
		setForecastThumbnailStyle(activeForecastTabMapTypeIndx);
		mw_showHide("forecastTabMap","block");
		imgReplace('forecastTabMapImg',mw_fcstArray[activeForecastTabDayIndx][activeForecastTabMapTypeIndx].largeURL);
	}	
	function deActivateForecastTab(){
		mw_showHide("fcstTab","none");		
		mw_showHide("forecastTabMap","none");
		deActivateForecastDailyTab();
		mw_showHide("forecastTabThumnail","none");	
	}	
	function activateForecastDailyTab(indx){
		deActivateForecastDailyTab();
		activeForecastTabDayIndx = indx;
		mw_showHide("forecastTabDailyTab"+indx,"block");	
	}
	function deActivateForecastDailyTab(){
		mw_showHide("forecastTabDailyTab0","none");
		mw_showHide("forecastTabDailyTab1","none");	
		mw_showHide("forecastTabDailyTab2","none");		
	}

	// util functions
	function mw_showHide(id,action) {
		if (document.getElementById) document.getElementById(id).style.display = action;
	}		 
	function goToInteractiveMapPage(animationFlag) {
		if(animationFlag) animationFlag = "&animation=" + animationFlag;
		else animationFlag = "";		
		
		var mapLayer = activeMapLayer;
		if(mapLayer) mapLayer = "&interactiveMapLayer=" + mapLayer;
		else mapLayer = "";
		
		var zoom = activeMapZoom;
		if(zoom) zoom = "&zoom=" + zoom;
		else zoom = "";
		
		if(isGold){					
			window.location = "http://gold.weather.com/weather/wxgold/mymaps/USCA0820"  + "?from=36hr_maps"+ zoom + animationFlag + mapLayer;
		}
		else window.location = "/outlook/travel/businesstraveler/map/interactive/USCA0820"  + "?from=36hr_maps"+ zoom + mapLayer + animationFlag;
	}	
	function showInteractiveGinsuMap(satOrRadar, zoom){
		activeMapZoom = zoom;
		// remove all children from element to force the png transparency issue
		var element = document.getElementById("interactiveMap");
		while (element.firstChild) { element.removeChild(element.firstChild);}
		document.getElementById("interactiveMap").appendChild(getInteractiveMap(satOrRadar, zoom));		
		
	}
	function getInteractiveMap(overlay, zoomLvl){return MapPane.create(36.6194, -121.9099, 440, 297, zoomLvl, 'r', overlay, 50);	}
	
	var tempX = 0;
	var tempY = 0;	
	function showBubble(){		document.getElementById("Bubblebar").style.display = "block";		 	}
	function hideBubble(){		document.getElementById("Bubblebar").style.display = "none";		 	}
	function mw_getMouseXY(e){
		if (isMinIE4) { // grab the x-y pos.s if browser is IE
			tempX = event.clientX + document.body.scrollLeft
			tempY = event.clientY + document.body.scrollTop
		}else {  // grab the x-y pos.s if browser is NS
			tempX = e.pageX
			tempY = e.pageY
		}
		// catch possible negative values in NS4
		if (tempX < 0){tempX = 0}
		if (tempY < 0){tempY = 0}  
			  
		var leftposition = 0;
		var topposition =  0;
		var xMapDivPosition = findPosX(document.getElementById("interactiveMap"));
		var yMapDivPosition = findPosY(document.getElementById("interactiveMap"));	
		leftposition = (tempX-xMapDivPosition);
		topposition = (tempY-yMapDivPosition);
 				  
				var xExtra = 0;
				var yExtra = 0;
				var yNextToTop = 50;
		
		document.getElementById("Bubblebar").style.left = leftposition+xExtra;
		document.getElementById("Bubblebar").style.top = topposition+yExtra; 
		var currentTabIsActive = document.getElementById("currentTab").style.display == "block" ? true : false;
		if(currentTabIsActive){
			if((tempX > xMapDivPosition) && (tempX < xMapDivPosition+440)){ 
				if((tempY > yMapDivPosition+1) && (tempY < yMapDivPosition+297)){	
					if((tempX > xMapDivPosition+340) && (tempX < xMapDivPosition+440)){document.getElementById("Bubblebar").style.left = leftposition-114+xExtra;								 								}
					if((tempY > yMapDivPosition+1) && (tempY < yMapDivPosition+50)){document.getElementById("Bubblebar").style.top = topposition+yNextToTop+yExtra;  						}
					document.getElementById("Bubblebar").style.display = "block";
					document.getElementById("Bubblebar").style.visibility = "visible";	   
				}
			}
			return true;
		}
		hideBubble();
	}		
	addEvent(document.getElementById("interactiveMap"),"mouseover", showBubble);
	addEvent(document.getElementById("interactiveMap"),"mousemove",mw_getMouseXY);
 	addEvent(document.getElementById("interactiveMap"),"mouseout",hideBubble);
	addEvent(window,'scroll',mapLoadImages);
	
	
	function setBackgroundImage (id, imageURL) {
		if (document.layers) document[id].background.src = imageURL == 'none' ? null : imageURL;
		else if (document.all) document.all[id].style.backgroundImage = imageURL == 'none' ? 'none' : 'url(' + imageURL + ')';
		else if (document.getElementById) document.getElementById(id).style.backgroundImage = imageURL == 'none' ? 'none' : 'url(' + imageURL + ')';
	}	
	var mapImagesLoaded = false;
	function mapLoadImages(){
		if(mapImagesLoaded) return;
		mapImagesLoaded = true;
		showCurrentTabMapfor(0);
		if(gnSiteMode == 'normal'){
			document.getElementById("timg0").setAttribute("src", mw_currArray[0].thumbURL);
			document.getElementById("timg1").setAttribute("src", mw_currArray[1].thumbURL);
			document.getElementById("timg2").setAttribute("src", mw_currArray[2].thumbURL);
			document.getElementById("timg3").setAttribute("src", mw_currArray[3].thumbURL);
		}else{		
			document.getElementById("timg0").style.display = 'none';
			document.getElementById("timg1").style.display = 'none';
			document.getElementById("timg2").style.display = 'none';
			document.getElementById("timg3").style.display = 'none';
		}
		setBackgroundImage("mapTopOn", "http://image.weather.com/web/common/nav/map_top_on.gif");
		setBackgroundImage("mapTopOff", "http://image.weather.com/web/common/nav/map_top_off.gif");
	}			
</script>








