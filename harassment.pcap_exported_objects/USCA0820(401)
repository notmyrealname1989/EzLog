<script type="text/javascript">
var warningMode = false;
var setBannerImage = "";
function triggerMode(){

	
}
function triggerHardCodedPage() {
	if(gnSiteMode=="severe") {
		setBannerImage = "http://i.imwx.com/web/common/banners/severes.jpg";
		document.write("<LINK REL='stylesheet' TYPE='text/css' HREF='http://c.imwx.com/web/common/header/stylesheet/severe.css'>");
	} else if(gnSiteMode =="alert") {
		setBannerImage = "http://i.imwx.com/web/common/banners/alerts.jpg";
		document.write("<LINK REL='stylesheet' TYPE='text/css' HREF='http://c.imwx.com/web/common/header/stylesheet/alert.css'>");
	}
	if(gnSiteMode=="severe" || gnSiteMode=="alert") {
		document.getElementById("MainHeaderImage").style.backgroundImage = "URL('"+setBannerImage+"')";
		warningMode = true;
		var severeDivValue = '<span id="severe_one">Severe</span><img src="http://i.imwx.com/web/blank.gif" alt="" width="3" height="1"><span id="severe_two">Storms</span>';
		writeDiv('SEVERE',severeDivValue);
	} else {
		document.getElementById("MainHeaderImage").style.backgroundImage = "URL('http://i.imwx.com/web/common/banners/summer/basic.jpg')";
	}
}
</script>
  

