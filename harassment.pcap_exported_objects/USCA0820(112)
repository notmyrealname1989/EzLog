{
	if(sNSevereItems.length > 0){
		var severeDivData = "";
		for (var i=0;i<sNSevereItems.length;i++){
		var severeSplit = sNSevereItems[i].split("^");
		severeDivData += '<A id="weather_news_'+i+'" href="'+severeSplit[1]+'?from=secondarynav">'+severeSplit[0]+'</A>';
			if(severeSplit[2] == 'yes'){
			severeDivData += '<span id="newicon">NEW!</span>';
			}
		}
		writeDiv('hdMI1', severeDivData);
		}
}
</script>
