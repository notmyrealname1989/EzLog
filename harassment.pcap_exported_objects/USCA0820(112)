<script>
var gotLocIDCookie = GetCookie("LocID");
//set the cookie with the newLocPres locid object.
var expdate = new Date ();
FixCookieDate(expdate);
expdate.setTime(expdate.getTime() + (24*60*60*1000*365));
var typeInfo = 1;
var newLocID = 'USCA0820';
if(typeInfo == 1 || typeInfo == 4)
{
SetCookie ("LocID",newLocID, expdate, "/", ".weather.com");
}
else
{
SetCookie ("LocID",newLocID+":"+typeInfo, expdate, "/", ".weather.com");
}

</script>



