<script language="javascript" type="text/javascript">

function notifyMoreRooms(theBox)
{
    if (theBox[theBox.selectedIndex].value == 9)
    {
        var msg = '';
        msg = 'If you need to book more than 9 rooms, please call\nthe hotel directly, 1-800-MEETING, or visit\nwww.meetings.ichotelsgroup.com. Bookings\nfor more than 9 rooms by the same guest or group\nfrom this current page will not be honored.\n';
        alert(msg);
    }
}


    
    
    function formSubmitJS(){
        document.searchForm.action='http://www.ichotelsgroup.com/h/d/hi/1/en/tpltp'
        document.searchForm.submit();
    }
    function formSubmitAddTP(){
        document.searchForm.action='http://www.ichotelsgroup.com/h/d/hi/1/en/tppraddtp'

    }
    


function cal_in_pop() {
	openFPCalendarCheckInAdvanced(document.searchForm, 'hi', '1', 'en', 'MM/dd/yyyy', 'false');
	document.getElementById('checkInTextBox').value = "";
	document.getElementById('checkInTextBox').focus();
	return false;
}

function cal_out_pop() {
	openFPCalendarCheckOutAdvanced(document.searchForm, 'hi', '1', 'en', 'MM/dd/yyyy', 'false');
	document.getElementById('checkOutTextBox').value = "";
	document.getElementById('checkOutTextBox').focus();
	return false;
}

function updateFields() {
	var nightsDropdown = document.getElementById('number_Nights');
	var nights = nightsDropdown.selectedIndex + 1;
	fieldAgreementNightsAdvanced(nights, 'MM/dd/yyyy');
}

function updateCheckOut() {
	var nightsDropdown = document.getElementById('number_Nights');
	if (nightsDropdown!=null && nightsDropdown!='undefined') {
		setCheckoutDateByNights(nightsDropdown, 'MM/dd/yyyy');
	}
}

function cal_checkin(){
	var checkInValue = document.getElementById('checkInTextBox').value;
	updateCheckinValue(checkInValue,'MM/dd/yyyy');
	return false;
}

</script>

