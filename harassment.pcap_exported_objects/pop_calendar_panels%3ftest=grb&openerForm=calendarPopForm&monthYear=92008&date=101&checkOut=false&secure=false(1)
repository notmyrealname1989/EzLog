    function reloadCalendar(openerForm, change) {
      var theForm = document.calControl;
      var month;
      var year;
      if((theForm.month.selectedIndex + change) < 0) {
        month = 11;
        year = parseInt(theForm.year[0].value) - 1;
      } else if((theForm.month.selectedIndex + change) > 11) {
        month = 0;
        year = theForm.year[1].value;
      } else {
        month = parseInt(theForm.month.value) + parseInt(change);
        year = parseInt(theForm.year.value);
      }
      if(theForm.year.selectedIndex == 1 && year != 2008 && month > 6) {
        alert("You may only place a reservation up to 350 days in advance");
        return false;
      }
      var jmonthYear = "" + month + year;
      document.location.href='/h/d/hi/1/en/pop_calendar_panels?test=grb&openerForm=' + openerForm + '&monthYear=' + jmonthYear + '&date=1&secure=false&dateFormat=null&checkOut=false';
	}

    function setToday(openerForm) {
      document.location.href='/h/d/hi/1/en/pop_calendar_panels?test=grb&openerForm=' + openerForm + '&monthYear=62008&date=22&checkOut=false&dateFormat=null&secure=false';
    }

    function setYear() {
      var theForm = document.calControl;
      if(6 > theForm.month.selectedIndex) {
        theForm.year.selectedIndex = parseInt(theForm.year.selectedIndex)+1;
      }
    }

    function getMonth() {
      var destForm = parent.document.forms.calendarPopForm;
      var sourceForm = document.calControl;
      return sourceForm.month.value;
    }

    function getDate() {
      var destForm = parent.document.forms.calendarPopForm;
      var sourceForm = document.calControl;
      return sourceForm.date.value;
    }

    function getYear() {
      var destForm = parent.document.forms.calendarPopForm;
      var sourceForm = document.calControl;
      return sourceForm.year.value;
    }

    function setCheckInPopDate( checkinDate, dateFormatIn ) {
      var destForm = parent.document.forms.calendarPopForm;
      var sourceForm = document.calControl;
      var outputDateFormat = 'null';
      
      
   
      var splitingDate = new Array(5);
      splitingDate = checkinDate.split('/');
      if(dateFormatIn == "MM/dd/yyyy"){
      	var date = parseInt(splitingDate[1], 10);
      	var month = parseInt(splitingDate[0], 10) - 1;
      }
      else if(dateFormatIn == "dd/MM/yyyy"){
      	var date = parseInt(splitingDate[0], 10);
      	var month = parseInt(splitingDate[1], 10) - 1;
      }
      else {
      	var date = parseInt(splitingDate[1], 10);
      	var month = parseInt(splitingDate[0], 10) - 1;
      }
      var year = splitingDate[2];
	  var monthYear = "" + month + year;

          for (var i = 0 ; i < destForm.checkInDate.options.length ; i++)
	        if (destForm.checkInDate.options[ i ].text == parseInt(date, 10))
	          destForm.checkInDate.options[ i ].selected = true;

	      for (var i = 0 ; i < destForm.checkInMonthYear.options.length ; i++)
	        if (destForm.checkInMonthYear.options[ i ].value == monthYear)
	          destForm.checkInMonthYear.options[ i ].selected = true;
        	populateCheckOut(destForm);

		if (parent.document.getElementById('calendar_pop_in')) {
			parent.document.getElementById('calendar_pop_in').style.display = "none";
			}
       }

    function setCheckOutPopDate( checkOutDate, dateFormatIn ) {
      var destForm = parent.document.forms.calendarPopForm;
      var sourceForm = document.calControl;

 
      var splitingDate = new Array(5);
      splitingDate = checkOutDate.split('/');
      if(dateFormatIn == "MM/dd/yyyy"){
      	var date = parseInt(splitingDate[1], 10);
      	var month = parseInt(splitingDate[0], 10) - 1;
      }
      else if(dateFormatIn == "dd/MM/yyyy"){
      	var date = parseInt(splitingDate[0], 10);
      	var month = parseInt(splitingDate[1], 10) - 1;
      }
      else {
      	var date = parseInt(splitingDate[1], 10);
      	var month = parseInt(splitingDate[0], 10) - 1;
      }
      var year = splitingDate[2];
	  var monthYear = "" + month + year;

 				for (var i = 0 ; i < destForm.checkOutDate.options.length ; i++) {
	      	if (destForm.checkOutDate.options[ i ].text == parseInt(date, 10)) {
	        	destForm.checkOutDate.options[ i ].selected = true;
	        }
				}

	      for (var i = 0 ; i < destForm.checkOutMonthYear.options.length ; i++ ) {
	        if (destForm.checkOutMonthYear.options[ i ].value == monthYear )
	          destForm.checkOutMonthYear.options[ i ].selected = true
	      }

		if (parent.document.getElementById('calendar_pop_out')) {
			parent.document.getElementById('calendar_pop_out').style.display = "none";
			}
       }

