 function calendarExit() {
	  if(parent.document.getElementById('calendar_pop_out')) {
            parent.document.getElementById('calendar_pop_out').style.display='none';
            parent.document.getElementById('calendar_pop_in').style.display='none';
       }
         else {
            parent.document.getElementById('calendar_pop_out').style.display='block';
            parent.document.getElementById('calendar_pop_in').style.display='block';
              }
        }

      function closeCal(inOut){
	     calendarExit();
