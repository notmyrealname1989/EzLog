    function FPreloadCalendar(openerForm, change) {
      var theForm = document.calControl;
      var month = parseInt(theForm.month.value);
      var year;
      if((month + parseInt(change)) < 0) {
        month = 11;
        year = parseInt(theForm.year.value) - 1;
      } else if((month + parseInt(change)) > 11) {
        month = 0;
        year = parseInt(theForm.year.value) + 1;
      } else {
        month = parseInt(theForm.month.value) + parseInt(change);
        year = parseInt(theForm.year.value);
      }
      if(year != 2008 && month > 6) {
        alert("You may only place a reservation up to 350 days in advance");
        return false;
      }
      var jmonthYear = "" + month + year;
      document.location.href='/h/d/hi/1/en/fpcal?test=grb&openerForm=' + openerForm + '&monthYear=' + jmonthYear + '&date=1&secure=false&checkOut=false';

    }

    function setToday(openerForm) {
      document.location.href='/h/d/hi/1/en/fpcal?test=grb&openerForm=' + openerForm + '&monthYear=62008&date=22&checkOut=false&secure=false';
    }

    function setYear() {
      var theForm = document.calControl;
      if(6 > theForm.month.value) {
        theForm.year = parseInt(theForm.year.value) + 1;
      }
    }

    function getMonth() {
      var destForm = window.opener.document.forms.searchForm;
      var sourceForm = document.calControl;
      return sourceForm.month.value;
    }

    function getDate() {
      var destForm = window.opener.document.forms.searchForm;
      var sourceForm = document.calControl;
      return sourceForm.date.value;
    }

    function getYear() {
      var destForm = window.opener.document.forms.searchForm;
      var sourceForm = document.calControl;
      return sourceForm.year.value;
    }

    function setCheckInDate( fulldate, datePattern ) {
      parent.populateCheckIn(fulldate, datePattern);
    }

    function setCheckOutDate( fulldate, datePattern ) {
      parent.populateCheckOutAdv(fulldate, datePattern);
    }

    function closeCal(inOut){
      parent.calendarExit();
    }

